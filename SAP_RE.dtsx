            DTS:Name="SqlStatementSource">"
update BISINTDATA..xinpvertrag set xvtEnde = null, xvtEnde_intf = '1' where xvtEnde = '20991231'
update BISINTDATA..xinpvertrag set xvertrNaechstesVertragsende = null, xvertrNaechstesVertragsende_intf = '1' where xvertrNaechstesVertragsende = '20991231'

update a 
 set xvtende = NULL
-- select a.*
 from " +@[User::cfgINTDB]+ "..xinpvertrag a
 join " +@[User::cfgINTDB]+ "..xinpKuendigungsoptionen b on a.xvertrag = b.xvertrag
 where xvtende &gt;= Getdate() and not xvtende = convert(datetime, '99991231',112)
 and not year(xkotermin) = 9999
 and dateadd(m,xkozeitraum, xkotermin) &gt;= Getdate()
 and a.xvertrEingangKuendigung is null
 and a.xvertrkuendzum is null

-- Verträge in Unbefristung mit Ende Festlaufzeit &lt; Heute
update vt
set xvtVertrEndeFestlaufzeit = null
from " +@[User::cfgINTDB]+ ".dbo.xinpVertrag vt
where xvtVertrEndeFestlaufzeit is not null
and xvtVertrEndeFestlaufzeit &lt; GETDATE() and xvtende is null

update a 
set xvtVertrEndeFestlaufzeit = Optende
from	" +@[User::cfgINTDB]+ ".dbo.xinpVertrag a
join (
	select xvertrag, xkoTermin, Optende from (
	select xvertrag, DATEADD(MONTH,xkozeitraum,DATEADD(DAY, -1, xkotermin)) as Optende, xkoTermin, xr=RANK() over(partition by xvertrag order by xkotermin desc)
	from " +@[User::cfgINTDB]+ ".dbo.xinpKuendigungsoptionen
	where xkoTermin &lt;= getdate() and DATEADD(MONTH,xkozeitraum,xkotermin)  &gt; GETDATE() and xkoptkuendtyp in('O','T')) qry where xr=1
	 ) opt on a.xVertrag = opt.xvertrag
WHERE (xvtEnde is null or xvtEnde &gt; GETDATE())
and (xvertrNaechstesVertragsende &lt; Optende or xvertrNaechstesVertragsende is null)
"</DTS:PropertyExpression>
