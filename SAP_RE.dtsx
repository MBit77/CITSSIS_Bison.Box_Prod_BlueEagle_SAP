select
  dtedit						= GETDATE()
, lUserkz						= 'interface'
, ldelete						= 0
, xobFond						= cast(SAP_VIBDBE.xfondsnr as varchar(10))
, xobGesellschaft				= cast(SAP_VIBDBE.xgesellnr as varchar(20))
, xObjekt						= cast(SAP_VIBDBE.xobjnr as varchar(50))
, xobBezeichnung				= cast(SAP_VIBDBE.XWETEXT as varchar (150))
, xobMatchcode					= cast(SAP_VIBDBE.XWETEXT as varchar(150))
, xobStrasse					= cast(SAP_ADRC.STREET as varchar(1024))
, xobjHnr						= cast(SAP_ADRC.HOUSE_NUM1 as varchar(10))
, xobPostleitzahl				= cast(SAP_ADRC.POST_CODE1 as varchar(50))
, xobOrt						= cast(SAP_ADRC.CITY1 as varchar(256))
--, xobjkey9						= '1' -- 1=Asset based, 9=Non-Asset based  /* siehe JOB Schnittstellen Aufruf*/
, xobFreeChar1					= cast(SAP_VIBDBE.SWENR as varchar(50))
, xobWirtschaftsuebergang		= BISDSYSDB.dbo.BIS_Convert_DATE(nullif(SAP_VIBDBE.DatumZugang,''), null)
, xobVKWirtschaftsueberg		= BISDSYSDB.dbo.BIS_Convert_DATE(nullif(SAP_VIBDBE.DatumAbgang,'99991231'), null)
, xobVKWirtschaftsueberg_intf	= '1'
, xobLand						= SAP_ADRC.COUNTRY
, xobvorsteueroption			= cast(opt.OPTRATE as decimal(5,2))
, xobWaehrung					= SAP_VIBDBE.WAERS
, xobBaujahr					= SAP_VIBDBU.YBAUJAHR
, xobLage						= SAP_TIV4E.xprefix+'_'+SAP_TIV4E.SOBJLAGE
-- select *
from dbo.SAP_VIBDBE
left join SAP_TIV4E on SAP_VIBDBE.xHubID = SAP_TIV4E.xHubID and SAP_VIBDBE.sobjlage = SAP_TIV4E.sobjlage and SAP_TIV4E.spras = 'D'
left join dbo.SAP_VZOBJECT on SAP_VIBDBE.INTRENO = SAP_VZOBJECT.ADROBJNR and SAP_VIBDBE.xHubID = SAP_VZOBJECT.xHubID
left join (select zr=row_number() over(partition by ADDRNUMBER order by DATE_TO desc) , * from dbo.SAP_ADRC) SAP_ADRC 
	on SAP_ADRC.zr=1 and SAP_vzobject.ADRNR = SAP_ADRC.ADDRNUMBER  and SAP_ADRC.xHubID = SAP_VZOBJECT.xHubID
left join 
(
	select OBJNR, OPTRATE, VALIDTO,  zr=ROW_NUMBER() over(partition by OBJNR order by VALIDTO desc) 
	from ( 
			select OBJNR,  VALIDTO =  BISDSYSDB.dbo.BIS_Convert_DATE(VALIDTO, null), OPTRATE =  bisdsysdb.dbo.BIS_Convert_FLOAT(opt.OPTRATE)
			-- select *
			from SAP_VIITOPTRATE opt 
			where ANCHPER = 'P' and isdate(BISDSYSDB.dbo.BIS_Convert_DATE(VALIDTO, null))  = 1
		) q
) opt on SAP_VIBDBE.OBJNR = opt.OBJNR and opt.zr=1 
--Baujahr aus den Gebäuden ermitteln
left join 
(
	select xHubID, BUKRS, SWENR, min(left(YBAUJAHR, 4)) as YBAUJAHR
	from SAP_VIBDBU
	group by xHubID, BUKRS, SWENR
) SAP_VIBDBU on SAP_VIBDBU.xHubID = SAP_VIBDBE.xHubID and SAP_VIBDBU.BUKRS = SAP_VIBDBE.BUKRS and SAP_VIBDBU.SWENR = SAP_VIBDBE.SWENR
where xobjnr is not null
-- wenn Variable AssetsOnlyInitial = 1, dann nur anlegen, wenn Objekt noch nicht vorhanden
and (
		(0 = 1 and xobjnr not in (select distinct xobjnr from BISIBOBDB.dbo.xlbiobjekte where ldelete = 0)) 
		OR
		(0 = 0)  
	)
