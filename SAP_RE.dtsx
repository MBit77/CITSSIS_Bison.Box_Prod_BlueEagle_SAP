              DTS:Expression="&quot;select distinct&#xA;  dtedit&#x9;&#x9;&#x9;= getdate()&#xA;, luserkz&#x9;&#x9;&#x9;= 'INTERFACE'&#xA;, ldelete&#x9;&#x9;&#x9;= '0'&#xA;, xggObjekt&#x9;&#x9;&#x9;= cast(obj.xobjnr as varchar(50))&#xA;, xGrundGebaeude&#x9;= cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(geb.SGENR,3) as varchar(50))&#xA;, xggMatchcode&#x9;&#x9;= cast(geb.XGETXT as varchar(150))&#xA;, xggBezeichnung&#x9;= cast(isnull(rtrim(adr.POST_CODE1 + ' ' + adr.CITY1), geb.XGETXT) as varchar(150))&#xA;, xggArt&#x9;&#x9;&#x9;= '0'&#xA;, xggBaujahr&#x9;&#x9;= left(nullif(geb.YBAUJAHR, ''), 4)&#xA;, xggPostleitzahl&#x9;= adr.POST_CODE1&#xA;, xggStrasse&#x9;&#x9;= isnull(adr.STREET, case when charindex('/', geb.XGETXT) &gt; 1 then substring(geb.XGETXT, charindex('/', geb.XGETXT)+2, 100) end)&#xA;, xggOrt&#x9;&#x9;&#x9;= isnull(adr.CITY1, case when (charindex('/', geb.XGETXT)&gt;0) then rtrim(isnull(adr.CITY1, left(geb.XGETXT, charindex('/', geb.XGETXT)-1))) end)&#xA;, xggGesellsch&#x9;&#x9;= obj.xGesellNr&#xA;, xggBeginn&#x9;&#x9;&#x9;= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(geb.VALIDFROM,''), nullif(obj.VALIDFROM,''))), NULL)&#xA;, xggEnde&#x9;&#x9;&#x9;= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(geb.VALIDTO, '99991231'), nullif(obj.VALIDFROM, '99991231'))), NULL)&#xA;, xggEnde_intf&#x9;&#x9;= '1'&#xA;, xgghnr&#x9;&#x9;&#x9;= adr.HOUSE_NUM1&#xA;-- select obj.*&#xA;from&#x9;&#x9;dbo.SAP_vibdbu geb&#xA;join&#x9;&#x9;dbo.SAP_vibdbe obj on geb.xHubid = obj.xHubId and geb.BUKRS = obj.BUKRS and geb.SWENR = obj.SWENR&#xA;left join&#x9;dbo.SAP_VZOBJECT vz on geb.xHubId = vz.xHubId and vz.ADROBJNR = geb.INTRENO and vz.ADROBJTYP = 'VI'&#xA;left join&#x9;dbo.SAP_Adrc adr on geb.xHubId = adr.xHubId and vz.ADRNR = adr.ADDRNUMBER&#xA;where obj.xobjnr is not null&#xA;&#xA;union&#xA;&#xA;select distinct&#xA;  dtedit&#x9;&#x9;&#x9;= getdate()&#xA;, luserkz&#x9;&#x9;&#x9;= 'INTERFACE'&#xA;, ldelete&#x9;&#x9;&#x9