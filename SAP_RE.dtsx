    <DTS:Executable
      DTS:refId="Package\Update Flächenarten (qm und stk)"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{BA2A68B9-214E-4B5C-9FC4-B8A50BA7B284}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update Flächenarten (qm und stk)"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E2C652A8-BD76-4FE9-A3B0-43C4FA828359}"
          SQLTask:SqlStatementSource="declare @units_qm table (xFlaeche varchar(50), xUmlageschluessel varchar(50))&#xA;&#xA;insert into @units_qm&#xA;select xflaeche, xUmlageschluessel&#xA;from &#xA;(&#xA;&#x9;select xFlaeche, xUmlageschluessel, xumWert, r=ROW_NUMBER() over (partition by xFlaeche, xUmlageschluessel order by xBeginn desc)&#xA;&#x9;from xinpUmlwerte&#xA;&#x9;where xUmlageschluessel in ('10', '20') -- qm, anzahl&#xA;) x &#xA;where r=1&#xA;and xumWert &gt; 0&#xA;&#xA;update f&#xA;set xflFlaechenart = &#xA;case &#xA;&#x9;when x.xFlaeche is not null then '98' -- Sonstige Fläche (qm)&#xA;&#x9;else '99' -- Sonstige Stück (#)&#xA;end&#xA;from xinpFlaeche f&#xA;left join @units_qm x on f.xFlaeche = x.xFlaeche and x.xUmlageschluessel = '10'&#xA;where xflFlaechenart = 'HIRAsap_9900' -- sonstige Nutzung&#xA;&#xA;&#xA;insert into xinpUmlwerte (dtedit, luserkz, ldelete, xFlaeche, xUmlageschluessel, xBeginn, xumWert, xumEinheit)&#xA;select dtedit=getdate(), luserkz='interface', ldelete=0, f.xFlaeche, xUmlageschluessel='20', xBeginn=f.xflZugangsdatum, xumWert=1, xumEinheit='stk'&#xA;from xinpFlaeche f&#xA;left join @units_qm x on f.xFlaeche = x.xFlaeche and x.xUmlageschluessel = '20'&#xA;where xflFlaechenart = '99' -- Sonstige Stück (#)&#xA;and x.xFlaeche is null&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Update Vertragsindex"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Task &quot;SQL ausführen&quot;"
      DTS:DTSID="{5c29d058-3f6a-4cee-aa65-2c4f195591fb}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update Vertragsindex"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{85fd5fce-e1d8-4d51-99b6-67ae18f92939}"
          SQLTask:SqlStatementSource="/*Anpassung Datumsangaben in Indexierungsvereinbarung&#xA;1) wenn Datum erste Prüfung = 01.01.1970 &gt; Update auf Vertragsbeginn&#xA;2) Anpassung Datum letzte Anpassung auf Beginn letzte / aktuellste NKM Sollstellung (die innerhalb der Vertragslaufzeit liegt)&#xA;*/&#xA;;with y as (&#xA;&#x9;&#x9;select &#xA;&#x9;&#x9;kv_land.xkeyalphanr as Land&#xA;&#x9;&#x9;, o.xobjnr&#xA;&#x9;&#x9;, f.xflaechennr&#xA;&#x9;&#x9;, f.xflaecheende&#xA;&#x9;&#x9;, v.xvertrnr&#xA;&#x9;&#x9;, v.xvertrbeginn&#xA;&#x9;&#x9;, v.xvertrende&#xA;&#x9;&#x9;, i.xindexnr&#xA;&#x9;&#x9;, vi.xvertridxersteprf, vi.xvertridxErsteanp, vi.xvertridxDatumLetzteAnp, vi.lid as vi_lid&#xA;&#x9;&#x9;, fs.xsollstbeginn, fs.xsollstvalue&#xA;&#x9;&#x9;, fs.xkeyalphanr as SollstArt&#xA;&#x9;&#x9;, R = ROW_NUMBER() over (partition by v.lid order by fs.xsollstbeginn desc)&#xA;&#x9;&#x9;from bisibobdb.dbo.xlbivertrag v&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbivertragidx vi on v.lid= vi.lparentid&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbiidxstamm i on i.lid = vi.xindexnr&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbiflaeche f on f.lid = v.xflaechennr&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbigrundgeb g on f.lparentid = g.lid&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbiobjekte o on o.lid = g.lparentid&#xA;&#x9;&#x9;inner join bisibobdb.dbo.xlbikeyval kv_land on kv_land.lid = o.xobjland&#xA;&#x9;&#x9;LEFT join &#xA;&#x9;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;select fs.xsollstbeginn, fs.lparentid, xkeyalphanr, xsollstvalue&#xA;&#x9;&#x9;&#x9;from BISIBOBDB.dbo.xlbiflsoll fs&#xA;&#x9;&#x9;&#x9;inner join bisibobdb.dbo.xlbikeyval kv_sollart &#xA;&#x9;&#x9;&#x9;on kv_sollart.lid = fs.xsollstart and kv_sollart.xkeyalphanr = '10'&#x9;&#xA;&#x9;&#x9;&#x9;where fs.ldelete = 0&#xA;&#x9;&#x9;&#x9;)fs &#xA;&#x9;&#x9;&#x9;&#x9;on fs.lparentid = f.lid&#xA;&#x9;&#x9;&#x9;&#x9;and fs.xsollstbeginn &gt;= v.xvertrbeginn&#xA;&#x9;&#x9;&#x9;&#x9;and fs.xsollstbeginn &lt;= ISNULL(v.xvertrende, '2099-01-01 00:00:00.000')&#xA;&#x9;&#x9;&#x9;&#x9;and fs.xsollstvalue &lt;&gt; 0 &#xA;&#x9;&#x9;&#x9;&#x9;where 1 = 1 &#xA;&#x9;&#x9;--and vi.xvertridxersteprf = '1970-01-01 00:00:00.000'&#xA;&#x9;&#x9;--and vi.xvertridxDatumLetzteAnp is null&#xA;&#x9;&#x9;and (YEAR(v.xvertrende) &gt;2020 or v.xvertrende is null)&#xA;&#x9;&#x9;and v.ldelete = 0 and vi.ldelete = 0 and f.ldelete = 0 and o.ldelete = 0&#xA;&#x9;&#x9;--and f.xflaechennr = '2300_00040_001_70005'&#xA;&#x9;)&#xA;, z as (&#xA;&#x9;select * from y&#xA;&#x9;where 1 = 1 and R = 1 and Land = 'DE' and xvertridxersteprf = '1970-01-01 00:00:00.000' and xvertridxDatumLetzteAnp is null --and xvertrbeginn &lt;&gt; '1970-01-01 00:00:00.000'&#xA;&#x9;union&#xA;&#x9;select * from y&#xA;&#x9;where 1 = 1 and R = 1 and Land &lt;&gt; 'DE' and (xsollstbeginn &gt; coalesce(xvertridxDatumLetzteAnp,xvertridxersteprf) or xvertridxersteprf &lt; xvertrbeginn)&#xA;&#x9;)&#xA;&#xA;update vi set&#xA;--select land, xflaechennr, xvertrnr, z.xvertrbeginn, z.xvertrende, z.xvertridxersteprf, z.xvertridxDatumLetzteAnp, xsollstbeginn,&#xA;/*ersteprüfung = minimumaus erste prüfung und Vertragsbeginn*/&#xA;xvertridxersteprf&#x9;&#x9;&#x9;= IIF(z.xvertridxersteprf &lt; z.xvertrbeginn, z.xvertrbeginn, z.xvertridxersteprf)&#xA;/*letzte Anpassung = aktuellestes gütig ab von NKM Sollstellung WENN Leer oder &lt; Vertragsbeginn oder &lt; letzte Sollstellung*/&#xA;, xvertridxDatumLetzteAnp&#x9;= IIF(z.xvertridxDatumLetzteAnp is null or z.xvertridxDatumLetzteAnp &lt;z.xvertrbeginn or z.xvertridxDatumLetzteAnp &lt; z.xsollstbeginn, z.xsollstbeginn, z.xvertridxDatumLetzteAnp)&#xA;from z&#xA;inner join BISIBOBDB.dbo.xlbivertragidx vi on vi.lid = vi_lid" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
