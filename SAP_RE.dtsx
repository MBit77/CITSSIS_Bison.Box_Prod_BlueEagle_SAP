                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= '0'
, xObjekt			= cast(obj.xobjnr as varchar(50))
, xAdressnummer		= cast(bp.PARTNER as varchar(50))
from dbo.SAP_VICNCN a with(nolock)
join dbo.SAP_VIBDOBJASS ass on a.xHubid = ass.xHubid and  '10' = OBJASSTYPE and a.OBJNR = ass.OBJNRSRC
join dbo.SAP_VIBDRO me on a.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
join	(	select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) 
			from dbo.SAP_vibpobjrel 
			-- externe role ist zu P03 (Vertragspartner gemappt)
			join temp_SAP_mapping_BP map on xPrefix +'_'+ ROLE = map.mapFrom
			where mapTo = 'P03' --and  PARTNER  = '0003011446'
		) bp on obj.xHubid = bp.xHubid and a.Intreno = bp.INTRENO and bp.R = 1

where obj.xobjnr is not null 

UNION 

select distinct
  dtedit		= getdate()
, luserkz		= 'INTERFACE'
, ldelete		= '0'
, xObjekt		= cast(obj.xobjnr as varchar(50))
, xAdressnummer	= cast(rtrim(obj.xobjnr) + ' 9999' as varchar(50))
from dbo.SAP_VIBDBE obj 
where xobjnr is not null</property>
