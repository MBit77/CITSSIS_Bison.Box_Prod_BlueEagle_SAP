                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with IndexSeries as ( 
select xHubID, INDEXSERIES, MAX(INDEXBASEYEAR) as INDEXBASEYEAR 
from   BISIMPORTDB.dbo.citSAP_Indexvereinbarung a 
where INDEXSERIES is not null 
group by xHubID, INDEXSERIES) 

select distinct
  dtedit		= getdate()
, luserkz		= 'Interface'
, ldelete		= 0
, xivindex		= cast(idxMap.mapTo as varchar(50))
, xivBasisjahr	= cast(a.BASJHR as int)
, xivJahr		= cast(a.INDJHR as int)
, xivMonat		= cast(a.INDMON as int)
, xivWert		= BISDSYSDB.dbo.BIS_Convert_DEC(a.INDPROZ)
, xivRW			= '1'
, xivDatum		= convert(date, rtrim(a.BASJHR) + rtrim(a.INDMON) + rtrim(a.INDTAG), 112)
--select *
from   dbo.SAP_t094P a
join   IndexSeries b on a.xHubID = b.xHubID and a.WBIND = b.INDEXSERIES and ltrim(a.BASJHR) = ltrim(b.INDEXBASEYEAR)
-- indexmapping
join (select mapFrom = xkeyalphanr, mapTo = BISIBOBDB.dbo.BISGET_Keyval_Alpha (xkeyHier01) from BISIBOBDB.dbo.fn_keyval ('ExtIndizes')) idxMap on idxMap.mapFrom = rtrim(a.xPrefix +'_'+ a.WBIND) + right(a.BASJHR,4)
and a.INDMON &lt;&gt; '00' and a.INDJHR &lt;&gt; '0000'</property>
