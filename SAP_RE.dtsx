              <DTS:VariableValue
                DTS:DataType="8">select distinct 
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
, xFlaeche				= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xUmlageschluessel		= cast(me.xPrefix +'_'+ uml.MEAS as varchar(50))
, xBeginn				= BISDSYSDB.dbo.BIS_Convert_DATE(COALESCE(nullif(uml.VALIDFROM,''), nullif(me.validfrom, ''), '19000101'), null)
, xumWert				= MEASVALUE
, xumEinheit			= me.xPrefix +'_'+ uml.MEASUNIT
-- select uml.*
from dbo.SAP_vibdro me
join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
join dbo.SAP_vibdmeas uml on me.xHubid = uml.xHubid and me.INTRENO = uml.INTRENO
where obj.xobjnr is not null
</DTS:VariableValue>
