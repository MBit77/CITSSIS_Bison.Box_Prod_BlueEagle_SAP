              <DTS:VariableValue
                DTS:DataType="8">SELECT DISTINCT 
  dtedit				= Getdate()
, luserkz				= 'Interface'
, ldelete				= 0
, xgeGesellschaftcode	= t1.BUKRS
, xgeLandSitz			= t1.LAND1
, xgeGesellschaftssitz	= t1.ORT01
, xgename				= t1.BUTXT
, xgeKurzbezeichnung	= t1.BUTXT
, xgesstatasset			= t1.BUKRS +'_'+ '09999'
, xgeBeginnJ			= CONVERT(int,ISNULL(gj.BUMON,1)) 
, xGesellwaehrung		= t1.WAERS
, xGesellFonds			= map.xFondsNr
--, t1.*
FROM dbo.sap_t001 t1
LEFT JOIN dbo.SAP_T009B gj on t1.xHubID = gj.xHubID and t1.PERIV = gj.PERIV and gj.POPER = '001'
left join (select xUnitCompanyCode, xFondsNr from BISIBOBDB.dbo.xIntfConfiguration where BISIBOBDB.dbo.BISGET_KEYisnum_ALPHA(xTypeOfDelivery) = ? and xImportTenant = 1) map on t1.BUKRS = map.xUnitCompanyCode</DTS:VariableValue>
