              <DTS:VariableValue
                DTS:DataType="8">SELECT 
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= 0 --case when xkoTermin &lt; getdate() then 1 else 0 end
, *
FROM
(
	-- 1.  Automatische Verlängerungen Individuell
	select
	xVertrag				= v.xVertrag
	, xFlaeche				= v.xvtFlaeche
	, xOptionsNr		= t1.RNRULENO + right(100+xopthelp,2)
	, xkoOptionsnehmerKZ= 'M'
	, xkoVorankuendigung= case	
							when rnnotyears &gt; 0 then rnnotyears * 12
							when rnnotmonths &gt; 0 then rnnotmonths
							else 0 
						  end
	, xkoTermin			=	case 
							-- erste autom. Verlängerung IMMER = Ende Festlaufzeit + 1 Tag
								when xopthelp = 1 
									then DATEADD(DAY, 1, xvtVertrEndeFestlaufzeit)
							-- weitere autom. Verlängerung in Abhängigkeit von Tag Ende Festlaufzeit a) wenn Ende Festlaufzeit = Monatsende
								when EOMONTH(xvtVertrEndeFestlaufzeit) = xvtVertrEndeFestlaufzeit 
									then dateadd( day, 1, EOMONTH(xvtVertrEndeFestlaufzeit, (case when rnextyears &gt; 0 then rnextyears * 12 when rnextmonths &gt; 0 then rnextmonths end) * (xopthelp-1)))
							-- weitere autom. Verlängerung in Abhängigkeit von Tag Ende Festlaufzeit b) wenn Ende Festlaufzeit = untermonatlich
								when EOMONTH(xvtVertrEndeFestlaufzeit) &lt;&gt; xvtVertrEndeFestlaufzeit 
									then DATEADD(DAY, 1, dateadd(month, (case when rnextyears &gt; 0 then rnextyears * 12 when rnextmonths &gt; 0 then rnextmonths end) * (xopthelp-1), xvtVertrEndeFestlaufzeit))
								end
	, xkoZeitraum		= case      
							when rnextyears &gt; 0 then rnextyears * 12
							when rnextmonths &gt; 0 then rnextmonths 
						  end
	, XKOPTKUENDTYP		= a.xPrefix  +'_'+ t1.RNRULETYPE
	-- select *      
	from dbo.SAP_VITMRN a
	join BISINTDATA.dbo.xinpVertrag v on a.INTRENO = v.xvertrExtID
	join		dbo.SAP_VITMRNRULEPR t1 on a.xHubid = t1.xHubid and a.intreno = t1.intreno
	----???? soll woll irgendwas bzgl. automat. Verlängeurngen erst nach Optionen machen oder so; ist aber nicht aktiv/ where - cond aktiv weil sonst Doppelung
	 LEFT JOIN 
		(select *, r=ROW_NUMBER() over (partition by xVertrag order by xkoTermin desc) from BISINTDATA..xinpKuendigungsoptionen) lstOpt 
			on lstOpt.xVertrag = v.xVertrag
	 cross join bisintdata..xinphelp
	where 1=1
	and a.rntype = '2' -- Option = individuell
	and a.TERMTYPE = '1010' -- Verlängerungsklausel
	and t1.RNRULETYPE = 'T' -- Automatische Verlängerungsoption
	and (xvtVertrEndeFestlaufzeit is not null or lstOpt.r = 1)-- nächstes Vertragsende
	and (lstOpt.r = 1 or lstOpt.r is null)

	union

	--2. Automatische Verlängerungen Regel
	select
	xVertrag				= v.xVertrag
	, xFlaeche				= v.xvtFlaeche
	, xOptionsNr		= t1.RNRULENO + right(100+xopthelp,2)
	, xkoOptionsnehmerKZ= 'M'
	, xkoVorankuendigung= case	
							when rnnotyears &gt; 0 then rnnotyears * 12
							when rnnotmonths &gt; 0 then rnnotmonths
							else 0 
						  end
	, xkoTermin			=	case 
							-- erste autom. Verlängerung IMMER = Ende Festlaufzeit + 1 Tag
								when xopthelp = 1 
									then DATEADD(DAY, 1, xvtVertrEndeFestlaufzeit)
							-- weitere autom. Verlängerung in Abhängigkeit von Tag Ende Festlaufzeit a) wenn Ende Festlaufzeit = Monatsende
								when EOMONTH(xvtVertrEndeFestlaufzeit) = xvtVertrEndeFestlaufzeit 
									then dateadd( day, 1, EOMONTH(xvtVertrEndeFestlaufzeit, (case when rnextyears &gt; 0 then rnextyears * 12 when rnextmonths &gt; 0 then rnextmonths end) * (xopthelp-1)))
							-- weitere autom. Verlängerung in Abhängigkeit von Tag Ende Festlaufzeit b) wenn Ende Festlaufzeit = untermonatlich
								when EOMONTH(xvtVertrEndeFestlaufzeit) &lt;&gt; xvtVertrEndeFestlaufzeit 
									then DATEADD(DAY, 1, dateadd(month, (case when rnextyears &gt; 0 then rnextyears * 12 when rnextmonths &gt; 0 then rnextmonths end) * (xopthelp-1), xvtVertrEndeFestlaufzeit))
								end
	, xkoZeitraum		= case      
							when rnextyears &gt; 0 then rnextyears * 12
							when rnextmonths &gt; 0 then rnextmonths 
						  end
	, XKOPTKUENDTYP		= a.xPrefix  +'_'+ t1.RNRULETYPE
	-- select *      
	from dbo.SAP_VITMRN a
	join BISINTDATA.dbo.xinpVertrag v on a.INTRENO = v.xvertrExtID
	join		dbo.SAP_VITMRNRULEPR t1 on a.xHubid = t1.xHubid and a.intreno = t1.intreno
	----???? soll woll irgendwas bzgl. automat. Verlängeurngen erst nach Optionen machen oder so; ist aber nicht aktiv/ where - cond aktiv weil sonst Doppelung
	 LEFT JOIN 
		(select *, r=ROW_NUMBER() over (partition by xVertrag order by xkoTermin desc) from BISINTDATA..xinpKuendigungsoptionen) lstOpt 
			on lstOpt.xVertrag = v.xVertrag
	 cross join bisintdata..xinphelp
	where 1=1
	and a.rntype = '1' -- Option = individuell
	and a.RNRULE &lt;&gt; '' -- es muss eine Regel vorhanden sein
	and a.TERMTYPE = '1010' -- Verlängerungsklausel
	and t1.RNRULETYPE = 'T' -- Automatische Verlängerungsoption
	and (xvtVertrEndeFestlaufzeit is not null or lstOpt.r = 1)-- nächstes Vertragsende
	and (lstOpt.r = 1 or lstOpt.r is null)
	
) x
where 1 = 1 and xkotermin &lt;= dateadd(year, 5, eomonth(getdate()))</DTS:VariableValue>
