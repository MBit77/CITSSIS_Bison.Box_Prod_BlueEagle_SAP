              <DTS:VariableValue
                DTS:DataType="8">SELECT 
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= case when xkoTermin &lt; getdate() then 1 else 0 end
, *
FROM
(
-- 1.  Optionen Individuell
select
/* RS - 20230426: wenn mehrere Verträge zu einem Hauptvertrag, dann erzeuge eine laufende Nummer */
--xVertrag			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR as varchar(50))
  xVertrag			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR + case when assAnz.lid is null or NbrVt = '' then '' else '_' + right('00'+ass.NbrVt,2) end as varchar(100))
, xFlaeche			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xOptionsNr		= t1.RNRULENO + right(100+xopthelp,2)
, xkoOptionsnehmerKZ= 'M'
, xkoVorankuendigung= case	
						when rnnotyears &gt; 0 then rnnotyears * 12
						when rnnotmonths &gt; 0 then rnnotmonths
						else 0 
					  end
, xkoTermin			= DATEADD(DAY, 1, 
						case 
						when year(BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL)) &gt; 2100 
							then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL) 
						else dateadd(m, ((xopthelp-1) 
						* 
						case
							when rnextyears &gt; 0 then rnextyears * 12
							when rnextmonths &gt; 0 then rnextmonths 
						end)
						+ isnull(t2.AnzMonth,0) , BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL)) 
					  end)
, xkoZeitraum		= case      
						when rnextyears &gt; 0 then rnextyears * 12
                        when rnextmonths &gt; 0 then rnextmonths 
					  end
, XKOPTKUENDTYP		= obj.xPrefix  +'_'+ t1.RNRULETYPE
-- select *      
from		dbo.SAP_VITMRN a
 join		dbo.SAP_VICNCN vtg on a.xHubId = vtg.xHubId and  a.intreno = vtg.intreno
 join		(
 SELECT xHubID, MANDT, OBJNRSRC, OBJNRTRG, ASSIGNONLYINFO
        , NbrVt = isnull(nullif(convert(varchar(10),ROW_NUMBER() over(partition by OBJNRSRC, OBJNRTRG order by isnull(validTo,'99991231'))),'1'),'')   
    FROM dbo.SAP_VIBDOBJASS
	WHERE OBJASSTYPE = '10'
 ) ass on a.xHubid = ass.xHubid and vtg.OBJNR = ass.OBJNRSRC
 left join (
	SELECT lid = 1, OBJNRSRC, OBJNRTRG, xhubid, MANDT, Anzahl = count(OBJASSTYPE)
	FROM dbo.SAP_VIBDOBJASS WHERE OBJASSTYPE = '10'
	GROUP BY OBJNRSRC, OBJNRTRG, xhubid, MANDT
	) assAnz on assAnz.OBJNRSRC = ass.OBJNRSRC and assAnz.OBJNRTRG = ass.OBJNRTRG and assAnz.xHubID = ass.xHubID and ass.MANDT = assAnz.MANDT and assAnz.Anzahl &gt; 1
 join		dbo.SAP_VIBDRO me on a.xHubId = me.xHubId and ass.OBJNRTRG = me.OBJNR
 join		dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
 join		dbo.SAP_VITMRNRULEPR t1 on a.xHubid = t1.xHubid and a.intreno = t1.intreno
 left join  
 (
	 select t1.xHubid, t1.intreno, t1.rnruleno
	, sum(t2.RNEXTCOUNT 
			* case 
				when t2.RNEXTYEARS &gt; 0 then t2.RNEXTYEARS * 12  
				when t2.RNEXTMONTHS &gt; 0 then t2.RNEXTMONTHS 
			  end
		  ) As AnzMonth     
	from 		SAP_vitmrnrulepr t1 
	join 		SAP_vitmrnrulepr t2 on t1.xHubid = t2.xHubid and t1.intreno = t2.intreno and t1.rnruleno &gt; t2.rnruleno 
	group by t1.xHubid, t1.intreno, t1.rnruleno 
 ) t2 on t1.xHubid = t2.xHubId and t1.intreno = t2.intreno and t1.rnruleno = t2.rnruleno
 cross join bisintdata..xinphelp
where obj.xobjnr is not null 
and a.rntype = '2' -- Option = individuell
and a.TERMTYPE = '1010' -- Verlängerungsklausel
and t1.RNRULETYPE = 'O' -- Verlängerungsoption
and ((xopthelp &lt;= RNEXTCOUNT) or (RNEXTCOUNT = 0 and xopthelp = 1)) -- RNEXTCOUNT = Anzahl der Verlängerungen
and  not RECNEND1ST is null -- nächstes Vertragsende

union

--2. Optionen Regel
select distinct
/* RS - 20230426: wenn mehrere Verträge zu einem Hauptvertrag, dann erzeuge eine laufende Nummer */
-- xVertrag			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR as varchar(50))
  xVertrag			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR + case when assAnz.lid is null or NbrVt = '' then '' else '_' + right('00'+ass.NbrVt,2) end as varchar(100))
, xFlaeche			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xOptionsNr		= t1.RNRULENO + right(100+xopthelp,2)
, xkoOptionsnehmerKZ= 'M'
, xkoVorankuendigung= case	
						when rnnotyears &gt; 0 then rnnotyears * 12
						when rnnotmonths &gt; 0 then rnnotmonths
						else 0 
					  end
, xkoTermin			= DATEADD(DAY, 1, case 
						when year(BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL)) &gt; 2100 
							then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL) 
						else dateadd(m, ((xopthelp-1) 
						* 
						case
							when rnextyears &gt; 0 then rnextyears * 12
							when rnextmonths &gt; 0 then rnextmonths 
						end)
						, BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL))
					  end)
, xkoZeitraum		= case      
						when rnextyears &gt; 0 then rnextyears * 12
                        when rnextmonths &gt; 0 then rnextmonths 
					  end
, XKOPTKUENDTYP		= obj.xPrefix  +'_'+ t1.RNRULETYPE 
from dbo.SAP_VITMRN a
join dbo.SAP_VICNCN vtg on a.xHubid = vtg.xHubid and  a.intreno = vtg.intreno
join		(
 SELECT xHubID, MANDT, OBJNRSRC, OBJNRTRG, ASSIGNONLYINFO
        , NbrVt = isnull(nullif(convert(varchar(10),ROW_NUMBER() over(partition by OBJNRSRC, OBJNRTRG order by isnull(validTo,'99991231'))),'1'),'')   
    FROM dbo.SAP_VIBDOBJASS
	WHERE OBJASSTYPE = '10'
 ) ass on a.xHubid = ass.xHubid and vtg.OBJNR = ass.OBJNRSRC
 left join (
	SELECT lid = 1, OBJNRSRC, OBJNRTRG, xhubid, MANDT, Anzahl = count(OBJASSTYPE)
	FROM dbo.SAP_VIBDOBJASS WHERE OBJASSTYPE = '10'
	GROUP BY OBJNRSRC, OBJNRTRG, xhubid, MANDT
	) assAnz on assAnz.OBJNRSRC = ass.OBJNRSRC and assAnz.OBJNRTRG = ass.OBJNRTRG and assAnz.xHubID = ass.xHubID and ass.MANDT = assAnz.MANDT and assAnz.Anzahl &gt; 1
join dbo.SAP_VIBDRO me on a.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
join dbo.SAP_TIVTMRNRULEPR t1 on a.xHubid = t1.xHubid and a.rnrule = t1.rnrule
cross join bisintdata..xinphelp
where obj.xobjnr is not null 
and a.rntype = '1' -- Option = Regel
and a.TERMTYPE = '1010' -- Verlängerungsklausel
and t1.RNRULETYPE = 'O' -- Verlängerungsoption
and a.RNRULE &lt;&gt; '' -- es muss eine Regel vorhanden sein
and ((xopthelp &lt;= RNEXTCOUNT) or (RNEXTCOUNT = 0 and xopthelp = 1)) -- RNEXTCOUNT = Anzahl der Verlängerungen
and  not RECNEND1ST is null -- nächstes Vertragsende
) x
