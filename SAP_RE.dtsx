                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
, xVertrag				= cast(obj.xobjnr +' '+ right(isnull(me.SGENR, 'G'+me.SGRNR), 3) + ' ' + right(me.SMENR,5) + ' ' + vtg.RECNNR as varchar(50))
, xvtFlaeche			= cast(obj.xobjnr +' '+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) + ' ' + right(me.SMENR,5) as varchar(50))
, xvtObjekt				= cast(obj.xobjnr as varchar(50))
, xvtAdressnummer		= cast(bp.PARTNER as varchar(50))
, xvtSollstellungstag	= '1'
, xvtLeerstehend		= case when isnull(ISVACANT,'') = 'X' then '1' else '0' end
, xvtBeginn				= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), case when ass.VALIDFROM is not NULL and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), ass.VALIDFROM), NULL) &gt; BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), isnull(vtg.RECNBEG,'01.01.1900')), NULL) then ass.VALIDFROM else vtg.RECNBEG end), NULL)
, xvtEnde				= nullif(
							BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), 
						  	case when BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), ass.VALIDTO), NULL) &lt; BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNENDABS), NULL) 
									then ass.VALIDTO
									else vtg.RECNENDABS
						  	end
						  	), NULL)
						  	, '99991231')
, xvtEnde_intf				= '1'
, xvtVertrEndeFestlaufzeit	= case	when BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL) is not null and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL) &gt;= getdate()
										then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL)
									else BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), verl.RNENDNEW), NULL)
							  end
, xvtVertrEndeFestlaufzeit_intf	= '1'
, xvertrBranche					= obj.xPrefix +'_'+ vtg.Industry
, xvtBemerkung					= cast(rtrim(vtg.RECNTXT) + case when fKndRgl.rglTxt is not null then  '   &lt;Individuelle Kündigungsregel vorhanden!&gt;   ' + fKndRgl.rglTxt end as varchar(1024))
, xvertrErstesVertrEnde			= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL)
, xvertrErstesVertrEnde_intf	= '1'
, xvertrstammvertrag			= obj.xobjnr + ' '+ vtg.RECNNR
, xvertrNaechstesVertragsende	= case	when BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNENDABS), NULL) is not null 
									and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNENDABS), NULL) &lt;= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), isnull(vtg.RECNEND1ST, '99991231')), NULL)
									and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNENDABS), NULL) &lt;= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), isnull(verl.RNENDNEW, '99991231')), NULL)
										then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNENDABS), NULL)
									when BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL) is not null and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL) &gt;= getdate()
										then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST), NULL)
									when BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), verl.RNENDNEW), NULL) is not null and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), verl.RNENDNEW), NULL) &gt;= getdate()
										then BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), verl.RNENDNEW), NULL)
									else null
								  end
, xvertrNaechstesVertragsende_intf	= '1'
--, xvertrKuendZum				= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNNOTPER), NULL)
