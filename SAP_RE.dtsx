    <DTS:Executable
      DTS:refId="Package\insert 0 debits for each debit position on lease begin"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{C7F7BA0D-02BB-4D62-8B4C-E76CA131A318}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="insert 0 debits for each debit position on lease begin"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E2C652A8-BD76-4FE9-A3B0-43C4FA828359}"
          SQLTask:SqlStatementSource="drop table if exists #tempSollstellungen&#xA; &#xA;select distinct xFlaeche,xsollstellungsart,xsmMonate,xsmWaehrung,xdtaSrcHub,xdtaSrcPFX&#xA;into #tempSollstellungen&#xA;from xinpSollmiete soll&#xA; &#xA; &#xA;drop table if exists #tempBeginne&#xA; &#xA;select xvtFlaeche,yBeginn,xVertrag&#xA;into #tempBeginne&#xA;from (&#xA;select xvtFlaeche,yBeginn = a.xvtBeginn,xVertrag from xinpVertrag a&#xA;where 1=1&#xA;and xvtBeginn is not null&#xA;/* RS - 20221215: es muss geklärt werden, ob Ausschluss von Leerstandsverträgen notwendig */&#xA;--and a.xVertrag not in (&#xA;--select xVertrag from (&#xA;--select r = ROW_NUMBER() OVER(PARTITION BY xvtFlaeche ORDER BY xvtBeginn DESC),* from xinpVertrag&#xA;--) a&#xA;--where r=1&#xA;--and a.xvtLeerstehend = 1&#xA;--)&#xA;&#xA;/* RS - 20230505: 0-Sollstellungen nur bei Vertragsbeginn */&#xA;--union&#xA; &#xA;--select xvtFlaeche,yBeginn = DATEADD(day,1,xvtEnde),xVertrag from xinpVertrag&#xA;--where 1=1&#xA;--and xvtEnde &lt;= getdate()&#xA;--and xvtEnde is not null&#xA;)x&#xA; &#xA;insert into xinpSollmiete (dtedit,luserkz,ldelete,xSollstellungsart,xFlaeche,xsmMonate,xBeginn,xsmWert,xsmWaehrung,xsollstDelete,xdtaSrcHub,xdtaSrcPFX)&#xA;SELECT distinct&#xA;&#x9;   dtedit&#x9;&#x9;&#x9; = getdate()&#xA;     , luserkz           = 'Interface'&#xA;     , ldelete           = 0&#xA;     , A.xSollstellungsart&#xA;     , A.xFlaeche&#xA;     , A.xsmMonate&#xA;     /* RS 20220808: Logik bei SAP nicht notwendig, Abstimmung mit SeRu erfolgt */&#xA;     , xBeginn           = case when 1=1 then yBeginn else DATEADD(d, 1-day(yBeginn), yBeginn) end&#xA;     , xsmWert           = 0.00&#xA;     , A.xsmWaehrung&#xA;     , xsollstDelete     = 0&#xA;     , A.xdtaSrcHub&#xA;     , A.xdtaSrcPFX&#xA;FROM(select * from #tempSollstellungen soll&#xA;join #tempBeginne vtg on vtg.xvtFlaeche = soll.xFlaeche&#xA;) A&#xA;--WHERE 1=1&#xA;left join BISINTDATA.dbo.xinpSollmiete inp&#xA;on inp.xFlaeche = A.xFlaeche&#xA;                and inp.xSollstellungsart =A.xSollstellungsart &#xA;                and inp.xBeginn = A.yBeginn&#xA;where inp.lid is null&#xA;/* RS - 20230505: externe Sollstellungen, die auf Mieteinheiten gepflegt sind, keine 0-Sollstellung schreiben */&#xA;and right(A.xSollstellungsart,CHARINDEX('_',REVERSE(A.xSollstellungsart))-1) not in ('RO')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
