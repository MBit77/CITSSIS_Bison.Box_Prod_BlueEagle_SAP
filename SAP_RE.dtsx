, xvtMibeso_intf				= '1'
 -- select *
 from			dbo.SAP_vicncn vtg with(nolock)
 join           dbo.SAP_vibdobjass ass on vtg.xHubid = ass.xHubid and '10' = ass.OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
 join           dbo.SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
 join		    dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
 join (select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) from dbo.SAP_vibpobjrel where ROLE = ?) bp on vtg.xHubid = bp.xHubid and vtg.Intreno = bp.INTRENO and bp.R = 1
 left join      dbo.SAP_vibdroocc oc on vtg.xHubid = oc.xHubid and  me.INTRENO = oc.INTRENO and vtg.BUKRS = oc.BUKRS and vtg.RECNNR = oc.OCCRECNNR
 left join		dbo.SAP_VICNNT kuend on vtg.xHubid = kuend.xHubid and kuend.INTRENO = vtg.INTRENO and NTACTIVE = 'X' and nullif(NTREVERSEDON, '00000000') is null and NTCATE = 'C' -- Contract
 -- Umsatzsteuer
 left join
( select *, zr=ROW_NUMBER() over(partition by xHubid, MANDT, OBJNR order by BISDSYSDB.dbo.BIS_Convert_DATE(isnull(VALIDFROM, '01.01.2019') , NULL)desc )  
	-- select * 
	from	dbo.SAP_VIITOPTRATE 
	where VALIDTO  &lt;&gt; '99991231' 
) tax  on vtg.xHubid = tax.xHubid and vtg.MANDT = tax.MANDT and vtg.OBJNR = tax.OBJNR and tax.zr = 1 
 -- Verlängerungen
 left join
 (
         select Verl.xHubId,  verl.INTRENO, verl.RNENDNEW
         from SAP_VICNRN Verl
         inner join (select distinct INTRENO, RNNO = MAX(RNNO) OVER (PARTITION BY INTRENO) from SAP_vicnrn where RNACTIVE = 'X' and nullif(RNACTIVATEDON,'') is not null) VerlMax
                 on Verl.INTRENO = VerlMax.INTRENO and Verl.RNNO = VerlMax.RNNO
         where Verl.RNACTIVE = 'X' and nullif(Verl.RNACTIVATEDON,'') is not null
 ) Verl on Verl.INTRENO = vtg.INTRENO
-- Kündigungsregeln
left join SAP_VITMNT kndRgl on kndRgl.xHubID = vtg.xHubID and kndRgl.INTRENO = vtg.INTRENO and kndRgl.NTTYPE in (1,2) and kndRgl.TERMTYPE = '1020'
-- feste Künd. Regel
left join SAP_TIVTMNTPROCT fKuenRgl on fKuenRgl.SPRAS = 'D' and fKuenRgl.NTPROC = kndRgl.NTPROC
-- individuelle Künd. Regel
left join 
(
	select distinct xHubID, INTRENO
		, STUFF((	select  ',' + XNTRULE 
					from [dbo].[SAP_VITMNTPROCREL] b 
					where a.intreno = b.intreno 
					and b.TERMTYPE = '1020'
					for xml path ('')), 1, 1, ''
				) as rglTxt 
	from [dbo].[SAP_VITMNTPROCREL] a
	where TERMTYPE = '1020' 
) fKndRgl on fKndRgl.xHubID = vtg.xHubID and fKndRgl.INTRENO = vtg.INTRENO
where obj.xobjnr is not null 
and isnull(ass.ASSIGNONLYINFO, '') != 'X'</property>
