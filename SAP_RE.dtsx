                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= '0'
, *
from
(
 select distinct
  xPersonenkonto	= cast(a.PARTNER as varchar(50))
, xpaMatchcode		= case bt0.TYPE
							when 1 then NAME_FIRST + isnull(' '+ NAME_LAST, '')
							when 2 then NAME_ORG1 + isnull(' '+ NAME_ORG2, '')
					  end
, xpaStrasse		= adr.STREET
, xpaLand			= adr.COUNTRY
, xpaPostleitzahl	= adr.POST_CODE1
, xpaOrt			= adr.CITY1
, xpahnr			= adr.HOUSE_NUM1
, xpaVorname		= case bt0.TYPE
						 when 1 then NAME_FIRST
						 when 2 then NULL
						 else isnull(nullif(NAME_FIRST,''), nullif(NAME_ORG1,'')) 
					  end
, xpaName			= case bt0.TYPE
						when 1 then NAME_LAST
						when 2 then NAME_ORG1 + isnull(' '+ NAME_ORG2, '')
						else isnull(nullif(NAME_LAST,''), nullif(NAME_ORG2,'')) 
					  end
, xpaAnrede			= case when bt0.TYPE = 2 then 'A03' end -- Firma
, [xperVorname2]	= a.ROLE -- für die Zuordnung der Personenrollen
-- select *
from 
(
	select distinct a.xHubId, bp.PARTNER, bp.mapTo as ROLE
	-- select *
	from dbo.SAP_VICNCN a with(nolock)
	join dbo.SAP_VIBDOBJASS ass on a.xHubid = ass.xHubid and  '10' = OBJASSTYPE and a.OBJNR = ass.OBJNRSRC
	join dbo.SAP_VIBDRO me on a.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
	join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
	join	(	select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) 
				from dbo.SAP_vibpobjrel 
				-- externe role ist zu P03 (Vertragspartner gemappt)
				join temp_SAP_mapping_BP map on xPrefix +'_'+ ROLE = map.mapFrom
				where mapTo = 'P03'
			) bp on a.xHubid = bp.xHubid and a.Intreno = bp.INTRENO and bp.R = 1
	where obj.xobjnr is not null 
) a 
join dbo.SAP_BUT000  bt0 on a.xHubId = bt0.xHubId and a.PARTNER = bt0.PARTNER
join (select xhubid,  PARTNER, ADDR_VALID_FROM = max(BISDSYSDB.dbo.BIS_Convert_DATE(cast(left(ADDR_VALID_FROM, 8) as char(8)), NULL)) from SAP_BUT020 where isdate(left(ADDR_VALID_FROM, 8)) = 1 group by xhubid, PARTNER) bt2Max on a.xHubid = bt2Max.xHubId and a.PARTNER = bt2Max.PARTNER
left join SAP_BUT020 bt2 on a.xHubId = bt2.xHubid and bt2Max.PARTNER = bt2.PARTNER and isdate(left(bt2.ADDR_VALID_FROM, 8)) = 1 and bt2Max.ADDR_VALID_FROM = BISDSYSDB.dbo.BIS_Convert_DATE(cast(left(bt2.ADDR_VALID_FROM, 8) as char(8)), NULL)
left join SAP_ADRC adr on a.xHubid = adr.xHubid and bt2.ADDRNUMBER = adr.ADDRNUMBER

union

SELECT DISTINCT
  xPersonenkonto	= cast(obj.xobjnr + ' 9999' as varchar(50))
, xpaMatchcode		= 'Leerstandsmieter'
, xpaStrasse		= null
, xpaLand			= null
, xpaPostleitzahl	= null
, xpaOrt			= null
, xpahnr			= null
, xpaVorname		= null
, xpaName			= 'Leerstandsmieter'
, xpaAnrede			= null
, [xperVorname2]	= null
from dbo.SAP_vibdbe obj 
where obj.xobjnr is not null 

union

 select distinct
  xPersonenkonto	= cast(a.PARTNER as varchar(50))
, xpaMatchcode		= case bt0.TYPE
							when 1 then NAME_FIRST + isnull(' '+ NAME_LAST, '')
							when 2 then NAME_ORG1 + isnull(' '+ NAME_ORG2, '')
					  end
, xpaStrasse		= adr.STREET
, xpaLand			= adr.COUNTRY
, xpaPostleitzahl	= adr.POST_CODE1
, xpaOrt			= adr.CITY1
, xpahnr			= adr.HOUSE_NUM1
, xpaVorname		= case bt0.TYPE
						 when 1 then NAME_FIRST
						 when 2 then NULL
						 else isnull(nullif(NAME_FIRST,''), nullif(NAME_ORG1,'')) 
					  end
, xpaName			= case bt0.TYPE
						when 1 then NAME_LAST
						when 2 then NAME_ORG1 + isnull(' '+ NAME_ORG2, '')
						else isnull(nullif(NAME_LAST,''), nullif(NAME_ORG2,'')) 
					  end
, xpaAnrede			= case when bt0.TYPE = 2 then 'A03' end -- Firma
, [xperVorname2]	= a.ROLE -- für die Zuordnung der Personenrollen
from
(
	select distinct obj.xHubId, bp.PARTNER, bp.mapTo as ROLE
	-- select *
	from dbo.SAP_VIBDBE obj --on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
	join	(	select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) 
				from dbo.SAP_vibpobjrel 
				-- externe role ist zu P03 (Vertragspartner gemappt)
				join temp_SAP_mapping_BP map on xPrefix +'_'+ ROLE = map.mapFrom
				where mapTo is not null
			) bp on obj.xHubid = bp.xHubid and obj.Intreno = bp.INTRENO and bp.R = 1
	where obj.xobjnr is not null 
) a 
join dbo.SAP_BUT000  bt0 on a.xHubId = bt0.xHubId and a.PARTNER = bt0.PARTNER
join (select xhubid,  PARTNER, ADDR_VALID_FROM = max(BISDSYSDB.dbo.BIS_Convert_DATE(cast(left(ADDR_VALID_FROM, 8) as char(8)), NULL)) from SAP_BUT020 where isdate(left(ADDR_VALID_FROM, 8)) = 1 group by xhubid, PARTNER) bt2Max on a.xHubid = bt2Max.xHubId and a.PARTNER = bt2Max.PARTNER
left join SAP_BUT020 bt2 on a.xHubId = bt2.xHubid and bt2Max.PARTNER = bt2.PARTNER and isdate(left(bt2.ADDR_VALID_FROM, 8)) = 1 and bt2Max.ADDR_VALID_FROM = BISDSYSDB.dbo.BIS_Convert_DATE(cast(left(bt2.ADDR_VALID_FROM, 8) as char(8)), NULL)
left join SAP_ADRC adr on a.xHubid = adr.xHubid and bt2.ADDRNUMBER = adr.ADDRNUMBER
) x</property>
