              DTS:EvaluateAsExpression="True"
              DTS:Expression="&quot;SELECT &#xA;  dtedit&#x9;&#x9;&#x9;= getdate()&#xA;, luserkz&#x9;&#x9;&#x9;= 'INTERFACE'&#xA;, ldelete&#x9;&#x9;&#x9;= case when xkoTermin &lt; getdate() then 1 else 0 end&#xA;, *&#xA;FROM&#xA;(&#xA;-- 1.  Optionen Individuell&#xA;select&#xA;/* RS - 20230426: wenn mehrere Verträge zu einem Hauptvertrag, dann erzeuge eine laufende Nummer */&#xA;--xVertrag&#x9;&#x9;&#x9;= cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(me.SMENR,5) +'&quot; + @[User::varIdentSeparator] + &quot;'+ vtg.RECNNR as varchar(50))&#xA;  xVertrag&#x9;&#x9;&#x9;= cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(me.SMENR,5) +'&quot; + @[User::varIdentSeparator] + &quot;'+ vtg.RECNNR + case when assAnz.lid is null or NbrVt = '' then '' else '_' + right('00'+ass.NbrVt,2) end as varchar(100))&#xA;, xFlaeche&#x9;&#x9;&#x9;= cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(me.SMENR,5) as varchar(50))&#xA;, xOptionsNr&#x9;&#x9;= t1.RNRULENO + right(100+xopthelp,2)&#xA;, xkoOptionsnehmerKZ= 'M'&#xA;, xkoVorankuendigung= case&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when rnnotyears &gt; 0 then rnnotyears * 12&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when rnnotmonths &gt; 0 then rnnotmonths&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else 0 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;  end&#xA;, xkoTermin&#x9;&#x9;&#x9;= DATEADD(DAY, 1, &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when year(BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNEND1ST),NULL)) &gt; 2100 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#