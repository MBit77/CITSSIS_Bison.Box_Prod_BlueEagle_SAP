        <SQLTask:SqlTaskData
          SQLTask:Connection="{85fd5fce-e1d8-4d51-99b6-67ae18f92939}"
          SQLTask:SqlStatementSource="if object_id ('BISimportDB.dbo.temp_SAP_mapping_BP') is not null drop table BISimportDB.dbo.temp_SAP_mapping_BP&#xA;select mapFrom=xkeyalphanr, mapTo=dbo.BISGET_Keyval_Alpha(xkeyHier01) &#xA;into BISimportDB.dbo.temp_SAP_mapping_BP&#xA;from dbo.fn_keyval('ExtPersonenarten') &#xA;where nullif(xkeyHier01, 0) is not null and ldelete = 0&#xA;&#xA;if object_id ('BISimportDB.dbo.temp_SAP_ObjTeam') is not null drop table BISimportDB.dbo.temp_SAP_ObjTeam&#xA;&#xA;select xobjnr&#xA;-- Objektteam&#xA;, xobjamcompany&#x9;&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'AM'  then PARTNER END)&#xA;, xobjbetreuerek&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'AMP'  then PARTNER END)&#xA;, xobjaccoutant&#x9;&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'BKP' then PARTNER END)&#xA;, xobjcaretaker&#x9;&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'HM'  then PARTNER END)&#xA;, xobjcommercialpm&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'KPM' then PARTNER END)&#xA;, xobjtechnicalpm&#x9;&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'TPM' then PARTNER END)&#xA;, xobjlettingmanager&#x9;&#x9;&#x9;= max(CASE WHEN mapTo = 'VRM' then PARTNER END)&#xA;into BISimportDB.dbo.temp_SAP_ObjTeam&#xA;from &#xA;(&#xA;select xobjnr, bp.*, R=ROW_NUMBER() over (partition by bp.mandt, bp.intreno, role order by bp.validfrom desc), mapTo&#xA;from BISimportDB.dbo.SAP_vibpobjrel bp&#xA;join BISimportDB.dbo.temp_SAP_mapping_BP map on xPrefix +'_'+ ROLE = map.mapFrom&#xA;join BISimportDB.dbo.SAP_VIBDBE on SAP_VIBDBE.xHubid = bp.xHubid and SAP_VIBDBE.MANDT = bp.MANDT and SAP_VIBDBE.Intreno = bp.INTRENO&#xA;where mapTo &lt;&gt; 'P03'&#xA;and xobjnr is not null&#xA;) x&#xA;where r=1&#xA;group by xobjnr " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
