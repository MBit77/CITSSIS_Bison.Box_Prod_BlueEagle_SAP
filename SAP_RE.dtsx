HYTHMUNIT  &#xA;, IndexErstePrf = (select min(bisdsysdb.dbo.BIS_Convert_DATE(cast(FIRSTINDEXDATE as char(50)), NULL)) from BISimportDB..SAP_VIAJOBJINDX where INTRENO = a.INTRENO)&#xA;into bisimportdb.dbo.citSAP_Indexvereinbarung&#xA;from &#x9;&#x9;dbo.SAP_vitmterm a &#xA;join&#x9;&#x9;dbo.##CONDADJ xx on a.xHubid = xx.xHubid and a.intreno = xx.INTRENO and a.TERMNO = xx.TERMNOAJ &#xA;join &#x9;&#x9;dbo.SAP_viajterm b on a.xHubid = b.xHubid and a.intreno = b.intreno and a.TERMNO = b.TERMNO &#xA;left join &#x9;SAP_viajrule c on a.xHubid = c.xHubid and a.intreno = c.intreno and a.termno = b.termno and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), c.VALIDTO), NULL) &gt;= GETDATE() &#xA;left join&#x9;SAP_viajobjindx d on a.xHubid = d.xHubid and b.intreno = d.intreno and b.termno = d.Termno and b.adjmrule = d.adjmrule and b.adjmsubrule = d.adjmsubrule and BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), d.VALIDTO), NULL) &gt;= GETDATE()&#xA;left join &#x9;SAP_viajspecindx e on a.xHubid = e.xHubid and b.intreno = e.intreno and b.termno = e.Termno and b.adjmrule = e.adjmrule and b.adjmsubrule = e.adjmsubrule   &#xA;left join &#x9;SAP_tivajspecindx f on a.xHubid = f.xHubid and b.ADJMSUBRULE = f.ADJMRULE &#xA;left join&#x9;SAP_TIVAJRULE g on a.xHubid = g.xHubid and b.ADJMSUBRULE = g.ADJMRULE&#xA;where a.TERMTYPE = '1300' --and b.ADJMSUBRULE = 'INDEXGEW.' &#xA;  and ISNULL(c.BLOCKREASON,'') = '' &#xA;  and(BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), b.validto), NULL) &gt;= Getdate() OR b.VALIDTO IS NULL)&#xA;  and not isnull(e.INDEXSERIES,f.INDEXSERIES) is NULL and not isnull(e.INDEXSERIES,f.INDEXSERIES) = 'KEINE'&#xA;&#xA;if object_id('tempdb.dbo.##CONDADJ') is not null drop table dbo.##CONDADJ &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
