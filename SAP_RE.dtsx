          <DTS:Variables>
            <DTS:Variable
              DTS:CreationName=""
              DTS:DTSID="{ec09bd6d-1609-43cb-a4fb-356f0020df80}"
              DTS:EvaluateAsExpression="True"
              DTS:Expression="&quot;select distinct &#xA;  dtedit    = getdate()&#xA;, luserkz    = 'INTERFACE'&#xA;, ldelete    = '0'&#xA;, xVertrag    = cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(me.SMENR,5) +'&quot; + @[User::varIdentSeparator] + &quot;'+ vtg.RECNNR as varchar(50))&#xA;, xkaFlaeche   = cast(obj.xobjnr +'&quot; + @[User::varIdentSeparator] + &quot;'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'&quot; + @[User::varIdentSeparator] + &quot;'+ right(me.SMENR,5) as varchar(50))&#xA;, xvtObjekt    = cast(obj.xobjnr as varchar(50))&#xA;, xkaSoll    = BISDSYSDB.dbo.BIS_Convert_DEC(isnull(solc.condvalue,soll.UNITPRICE)) &#xA;, xkaArt    = cast(obj.xPrefix +'_'+ sollTxt.CONDTYPE as varchar(50))&#xA;, xkautBem    = rtrim(stmVtg.RECNTXT)&#xA;, xkautSollWaehrung  = soll.CONDCURR&#xA;, xkaWaehrung   = soll.CONDCURR&#xA;, xkauthaben   = 0  &#xA;-- select *&#xA;from SAP_VICNCN stmVtg with(nolock)&#xA;join SAP_VICNCN Vtg on stmVtg.RECNNRCOLLECT = vtg.RECNNR and stmVtg.RECNBUKRSCOLLECT = vtg.BUKRS&#xA;join SAP_VICDCOND soll on stmVtg.INTRENO = soll.INTRENO&#xA;left join dbo.SAP_VICDCONDCALC solc on soll.CONDGUID = solc.CONDGUID &#xA;join SAP_TIVCDCONDTYPET sollTxt on soll.CONDTYPE = sollTxt.CONDTYPE&#xA;inner join  (select *, VALIDFROMX = convert(datetime,nullif(VALIDFROM,'00.00.0000')) , VALIDTOX = convert(datetime,nullif(VALIDTO,'00.00.0000')) &#xA;             from dbo.SAP_VIBDOBJASS where '10' = OBJASSTYPE&#xA;   )  ass on vtg.OBJNR = ass.OBJNRSRC and vtg.xHubID = ass.xHubID&#xA;inner join dbo.SAP_VIBDRO me on ass.OBJNRTRG = me.OBJNR and ass.xHubID = me.xHubID&#xA;join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR&#xA;where obj.xobjnr is not null&#xA;and sollTxt.XCONDTYPEL like '%kaut%'&#xA;&quot;"
              DTS:IncludeInDebugDump="2345"
              DTS:Namespace="User"
              DTS:ObjectName="varSQL">
              <DTS:VariableValue
                DTS:DataType="8">select distinct 
  dtedit    = getdate()
, luserkz    = 'INTERFACE'
, ldelete    = '0'
, xVertrag    = cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR as varchar(50))
, xkaFlaeche   = cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xvtObjekt    = cast(obj.xobjnr as varchar(50))
, xkaSoll    = BISDSYSDB.dbo.BIS_Convert_DEC(isnull(solc.condvalue,soll.UNITPRICE)) 
, xkaArt    = cast(obj.xPrefix +'_'+ sollTxt.CONDTYPE as varchar(50))
, xkautBem    = rtrim(stmVtg.RECNTXT)
, xkautSollWaehrung  = soll.CONDCURR
, xkaWaehrung   = soll.CONDCURR
, xkauthaben   = 0  
-- select *
from SAP_VICNCN stmVtg with(nolock)
join SAP_VICNCN Vtg on stmVtg.RECNNRCOLLECT = vtg.RECNNR and stmVtg.RECNBUKRSCOLLECT = vtg.BUKRS
join SAP_VICDCOND soll on stmVtg.INTRENO = soll.INTRENO
left join dbo.SAP_VICDCONDCALC solc on soll.CONDGUID = solc.CONDGUID 
join SAP_TIVCDCONDTYPET sollTxt on soll.CONDTYPE = sollTxt.CONDTYPE
inner join  (select *, VALIDFROMX = convert(datetime,nullif(VALIDFROM,'00.00.0000')) , VALIDTOX = convert(datetime,nullif(VALIDTO,'00.00.0000')) 
             from dbo.SAP_VIBDOBJASS where '10' = OBJASSTYPE
   )  ass on vtg.OBJNR = ass.OBJNRSRC and vtg.xHubID = ass.xHubID
inner join dbo.SAP_VIBDRO me on ass.OBJNRTRG = me.OBJNR and ass.xHubID = me.xHubID
join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
where obj.xobjnr is not null
and sollTxt.XCONDTYPEL like '%kaut%'
</DTS:VariableValue>
