      DTS:LocaleID="-1"
      DTS:ObjectName="clear last debit for each unit if value is 0"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E2C652A8-BD76-4FE9-A3B0-43C4FA828359}"
          SQLTask:SqlStatementSource="drop table if exists #debits2Delete;&#xA; &#xA;select lid into #debits2Delete from (&#xA;select *,r=ROW_NUMBER() OVER(PARTITION BY xFlaeche ORDER BY xBeginn DESC, xsmWert) from xinpSollmiete&#xA;where xSollstellungsart in ('10')&#xA;) x where x.r = 1&#xA; &#xA;/*&#xA;RS - 20230418:&#xA;- Bereinigung nur für Nettokaltmiete, da sonst sinnvolle 0-Sollstellungen eventuell nicht berücksichtigt werden&#xA;- evtl. Anpassung Logik, sodass zu jeder Fläche das Datum der letzten Sollstellung ermittelt wird und nur für diese Datum 0-Sollstellungen bereinigt werden&#xA;*/&#xA;delete&#xA;--select *&#xA;from xinpSollmiete&#xA;output deleted.*&#xA;where lid in (select lid from #debits2Delete) and xsmWert = 0.0 and xSollstellungsart in ('10')&#xA; &#xA;/*&#xA;select distinct yBeginn = max(xBeginn) OVER(PARTITION BY xFlaeche),xflaeche&#xA;into #debits2Delete&#xA;from xinpSollmiete&#xA;where xSollstellungsart is not null&#xA;go&#xA; &#xA;--select * from #debits2Delete&#xA; &#xA;delete s&#xA;output deleted.*&#xA;--select *&#xA;from xinpSollmiete s&#xA;join #debits2Delete # on #.xFlaeche = s.xFlaeche and #.yBeginn = s.xBeginn&#xA;where s.xsmWert = 0.0&#xA;and s.xSollstellungsart in ('10')&#xA;*/&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
