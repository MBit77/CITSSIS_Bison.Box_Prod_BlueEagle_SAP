                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
  dtedit		= getdate()
, luserkz		= 'Interface'
, ldelete		= 0
, xkvdatum		= getdate()
, *
from
(
	select distinct
	  xkhfield		=  cast('ExtSollstellungsarten' as varchar(50))
	, xkvAlphacode	=  cast(soll.xPrefix + '_' +  soll.CONDTYPE as varchar(255)) + '_VAT'
	, xkvMatchcode  =  cast(rtrim(sollTxt.XCONDTYPEL)  + ' (value added tax)' as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	-- select *
	from dbo.SAP_VICDCOND soll
	join dbo.SAP_TIVCDCONDTYPET sollTxt on soll.CONDTYPE = sollTxt.CONDTYPE
	where isnull(CONDDELETE, '') != 'X'
	and sollTxt.SPRAS = 'D'

	union

	select distinct 
	  xkhfield		=  cast('ExtSollstellungsarten' as varchar(50))
	, xkvAlphacode	=  cast(soll.xPrefix +'_'+ soll.xSollstellungsart+'_CN' as varchar(255))
	, xkvMatchcode  =  cast(sollTxt_D.XCONDTYPEL + ' (contract)' as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	--, xkvMatchcode_E  =  cast(sollTxt_E.XCONDTYPEL as varchar(255))
	from cit_SollmieteVertrag soll
	join dbo.SAP_TIVCDCONDTYPET sollTxt_D on soll.CONDTYPE = sollTxt_D.CONDTYPE and sollTxt_D.SPRAS = 'D'
	left join dbo.SAP_TIVCDCONDTYPET sollTxt_E on soll.CONDTYPE = sollTxt_E.CONDTYPE and sollTxt_E.SPRAS = 'E'

	union

	select distinct 
	  xkhfield		=  cast('ExtSollstellungsarten' as varchar(50))
	, xkvAlphacode	=  cast(soll.xPrefix +'_'+ soll.xSollstellungsart+'_RO' as varchar(255))
	, xkvMatchcode  =  cast(sollTxt_D.XCONDTYPEL + ' (rental object)' as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	--, xkvMatchcode_E  =  cast(sollTxt_E.XCONDTYPEL as varchar(255))
	from cit_SollmieteFlaeche soll
	join dbo.SAP_TIVCDCONDTYPET sollTxt_D on soll.CONDTYPE = sollTxt_D.CONDTYPE and sollTxt_D.SPRAS = 'D'
	left join dbo.SAP_TIVCDCONDTYPET sollTxt_E on soll.CONDTYPE = sollTxt_E.CONDTYPE and sollTxt_E.SPRAS = 'E'

	union

	select distinct
	  xkhfield		= cast('ExtUmlagen' as varchar(50))
	, xkvAlphacode	= cast(uml.xPrefix + '_' + uml.MEAS as varchar(255))
	, xkvMatchcode  = cast(rtrim(XMMEAS) as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	-- select *
	from dbo.SAP_TIVBDMEAST uml
	where SPRAS = 'D'

	union

	select distinct
	  xkhfield		= cast(x.KeyHead as varchar(50))
	, xkvAlphacode	= cast(NuArt.xPrefix+'_' + NuArt.SNUNR as varchar(255))
	, xkvMatchcode  = cast(rtrim(NuArt.XMBEZ) as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	-- select *
	from dbo.SAP_TIV0A NuArt
	cross join (select 'ExtFlaechenarten' as KeyHead union select 'Flaechenarten') x
	where SPRAS = 'D'

	union

	select distinct
	  xkhfield		= cast('Kündigungsregel' as varchar(50))
	, xkvAlphacode	= cast(xPrefix + '_' + NTPROC as varchar(255))
	, xkvMatchcode  = cast(rtrim(XNTPROC) as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	-- select *
	from dbo.SAP_TIVTMNTPROCT 

	union

	select distinct
	  xkhfield		= cast('Kündigungsregel' as varchar(50))
	, xkvAlphacode	= cast(xPrefix + '_' + '0000' as varchar(255))
	, xkvMatchcode  = cast(rtrim('&lt;individuelle Fristregel&gt;') as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	from dbo.SAP_TIVTMNTPROCT 

	union

	select distinct 
	  xkhfield		= cast('ExtIndizes' as varchar(50))
	, xkvAlphacode	= cast(ix.xPrefix +'_'+ ix.INDEXSERIES + isnull(rtrim(ix.INDEXBASEYEAR), '') as varchar(255))
	, xkvMatchcode  = cast(ixTxt.INDTXT + ' - ('+ix.INDEXBASEYEAR+')' as varchar(255))
	, xkvUpdate		= '2' -- insert und update
	-- select *
	-- Verknüpfung auf die Vertragsdaten, damit nur verwendete Indices angelegt werden
	from dbo.SAP_vicncn vtg with(nolock)
	join dbo.SAP_vibdobjass ass on vtg.xHubid = ass.xHubid and '10' = ass.OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
	join dbo.SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
	join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR join dbo.SAP_TIV26 t26 on vtg.RECNTYPE = t26.SMVART
	join dbo.citSAP_Indexvereinbarung ix on vtg.INTRENO = ix.INTRENO and vtg.xHubid = ix.xHubid
	join dbo.SAP_T094T ixTxt on ix.xHubID = ixTxt.xHubID and ix.INDEXSERIES = ixTxt.WBIND and ixTxt.SPRAS = 'D'

	union

	select distinct
	  xkhfield		=  cast('ExtKautionsart' as varchar(50))
	, xkvAlphacode	=  cast(sollTxt.xPrefix +'_'+ sollTxt.CONDTYPE as varchar(255))
	, xkvMatchcode  =  cast(sollTxt.XCONDTYPEL as varchar(255))
	, xkvUpdate		= '0' -- nur insert
	-- select *
	from SAP_VICDCOND soll 
	join SAP_TIVCDCONDTYPET sollTxt on soll.CONDTYPE = sollTxt.CONDTYPE
	where sollTxt.XCONDTYPEL like '%kaut%'
	and SPRAS = 'D'

	union
	
	select distinct
	  xkhfield		=  cast('ExtKautionsart' as varchar(50))
	, xkvAlphacode	=  cast(xPrefix +'_'+ ad_data as varchar(255))
	, xkvMatchcode  =  cast(xPrefix +'_'+ ad_data as varchar(255))
	, xkvUpdate		= '0' -- nur insert
	-- select *
	from SAP_VIAD_DATA_KAUTIONEN 
	where var_id = '0000000032' 
	and ad_data &lt;&gt; '' and ad_data is not null
	-- Diese Kautionsarten stehen in SAP über eine Dropdown - Auswahl zur Verfügung
	and ad_data
	in
	(
	'keine vereinbart',
	'BKXL-Kaution',
	'Cashkonto D&amp;R',
	'Barkaution',
	'Bankbürgschaft',
	'Escrow',
	'Konzernbürgschaft',
	'Sparbuch/Verpfändung',
	'Patronatserklärung',
	'Liegt noch nicht vor'
	)

	union

	select distinct
	  xkhfield		=  cast('ExtVertragsbranche' as varchar(50))
	, xkvAlphacode	=  cast(xPrefix +'_'+ IND_SECTOR as varchar(255))
	, xkvMatchcode  =  cast(max(text) as varchar(255))
	, xkvUpdate		= '0' -- nur insert
	-- select *
	from SAP_TB038B with(nolock)
	where spras = 'D' 
	and ind_sector is not null 
	and ind_sector != '' 
	group by xPrefix, IND_SECTOR

union
	select distinct
	  xkhfield		=  cast('Objektlage' as varchar(50))
	, xkvAlphacode	=  cast(xprefix+'_'+SOBJLAGE as varchar(255))
	, xkvMatchcode  =  cast(XOBJLAGE as varchar(255))
	, xkvUpdate		= '0' -- nur insert
	-- select *
	from SAP_TIV4E 
	where spras = 'D'

	/*
	select distinct
	  xkhfield		=  cast('ExtVertragsbranche' as varchar(50))
	, xkvAlphacode	=  cast(obj.xPrefix +'_'+ vtg.INDUSTRY as varchar(255))
	, xkvMatchcode  =  cast(obj.xPrefix +'_'+ vtg.INDUSTRY as varchar(255))
	, xkvUpdate		= '0' -- nur insert
	-- select *
	from dbo.SAP_vicncn vtg with(nolock)
	join dbo.SAP_vibdobjass ass on vtg.xHubid = ass.xHubid and '10' = ass.OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
	join dbo.SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
	join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
	*/
) keys
where xkvAlphacode is not null</property>
