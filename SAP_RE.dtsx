          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"
declare @KeyHead_TypeOfDataDelivery int = (select lid from dbo.xlbikeyhead where xkeyhmc = 'TypeOfDataDelivery')
declare @KeyHead_PrefixAccounting	int = (select lid from dbo.xlbikeyhead where xkeyhmc = 'PrefixAccounting')
declare @KeyHead_TypeOfMapping		int = (select lid from dbo.xlbikeyhead where xkeyhmc = 'TypeOfMapping')

declare @KeyVal_TypeOfDataDelivery  int = (select top 1 lid from dbo.xlbikeyval where lparentid = @KeyHead_TypeOfDataDelivery and xkeyalphanr in ('" +@[User::cfgTypeOfDelivery]+ "'))
declare @KeyVal_TypeOfMapping		int = (select lid from dbo.xlbikeyval where lparentid = @KeyHead_TypeOfMapping and xkeyalphanr = '1' /*Objekt*/)


insert into dbo.xIntfConfiguration (dtedit, luserid, ldelete, xHubID, xPrefix, xUnitAssetCode, xTypeOfDelivery, xComment, xPeriodValidFrom, xPeriodValidTo, xTypeofMapping, xUnitCompanyCode)
select dtedit = getdate(), luserid = 0, ldelete = 0, xHubID = inp.xHUBID, xPrefix = prf.lid, xUnitAssetCode = SWENR, xTypeOfDelivery = @KeyVal_TypeOfDataDelivery, xComment = inp.XWETEXT, xPeriodValidFrom = '190001', xPeriodValidTo = '999912', xTypeofMapping = @KeyVal_TypeOfMapping, xUnitCompanyCode = inp.BUKRS
from " +@[User::cfgIMPDB]+ ".dbo.SAP_VIBDBE inp
left join xIntfConfiguration map on inp.xHUBID = map.xHubID and rtrim(inp.SWENR) = rtrim(map.xUnitAssetCode) and map.xTypeOfDelivery = @KeyVal_TypeOfDataDelivery and inp.BUKRS = map.xUnitCompanyCode
cross apply (select lid from dbo.xlbikeyval where lparentid = @KeyHead_PrefixAccounting and xkeymc = inp.xHUBID +' - '+ '" +@[User::cfgTypeOfDelivery]+ "') prf
where map.lid is null
and inp.xHUBID is not null
