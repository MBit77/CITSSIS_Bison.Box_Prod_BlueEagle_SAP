          SQLTask:SqlStatementSource="-- 1. Verträge bestimmen &#xA;If OBJECT_ID('dbo.cit_SAPVertrag') is not null drop table dbo.cit_SAPVertrag &#xA;go &#xA;select distinct &#xA;  vt.MANDT, vt.INTRENO as INTRENOVT, RECNNR, vt.OBJNR as vtOBJNR, vt.IMKEY as vtIMKEY &#xA;, me.OBJNR as meOBJNR &#xA;, me.BUKRS, me.SWENR, me.SMENR, me.IMKEY as meIMKEY, me.SGENR, me.SGRNR &#xA;, NbrVt &#xA;, vt.RECNENDABS&#xA;, case when ass.VALIDFROM is not NULL and ass.VALIDFROM &gt; isnull(recnbeg,'19000101') then CONVERT(datetime,ass.VALIDFROM,120)&#xA;          else  convert(datetime,isnull(recnbeg,'19000101'),120) end as xvtBeginn&#xA;, case when convert(datetime,ass.VALIDTO,120) &lt; convert(datetime,recnendabs,120)&#xA;             then nullif(convert(datetime,ass.VALIDTO,120),'99991231') else nullif(convert(datetime,recnendabs,120),'99991231') end as xvtEnde&#xA;, isnull(VALIDFROMX,'17530101')  as OBJVALIDFROMX , VALIDTOX as OBJVALIDTOX  &#xA;--, xvertrag = me.BUKRS + ' ' + right('000000' + me.SWENR,6) + ' ' + me.smenr + ' ' + vt.RECNNR + case when NbrVt &gt;1 then ' ' + right(100+NbrVt,2) else '' end &#xA;--, xFlaeche = me.BUKRS + ' ' + right('000000' + me.SWENR,6) + ' ' + me.smenr &#xA;, xvertrag&#x9;= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vt.RECNNR as varchar(50))&#xA;, xFlaeche&#x9;= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))&#xA;, me.xPrefix&#xA;into dbo.cit_SAPVertrag  &#xA;-- select *&#xA;from&#x9;&#x9;dbo.SAP_VICNCN vt with(nolock)&#xA;inner join&#x9;(&#xA;&#x9;&#x9;&#x9;select *&#xA;&#x9;&#x9;&#x9;&#x9;, VALIDFROMX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(VALIDFROM,''), null) &#xA;&#x9;&#x9;&#x9;&#x9;, VALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(VALIDTO,''), null)&#xA;&#x9;&#x9;&#x9;&#x9;, NbrVt = ROW_NUMBER() over(partition by OBJNRSRC, OBJNRTRG order by validTo)    &#xA;&#x9;&#x9;&#x9;from dbo.SAP_VIBDOBJASS &#xA;&#x9;&#x9;&#x9;where '10' = OBJASSTYPE&#xA;&#x9;&#x9;&#x9;)  ass on  vt.OBJNR = ass.OBJNRSRC &#xA;join &#x9;&#x9;dbo.SAP_TIV26 t26 on vt.RECNTYPE = SMVART &#xA;join &#x9;&#x9;dbo.SAP_VIBDRO me on ass.OBJNRTRG = me.OBJNR&#xA;join&#x9;&#x9;dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR &#xA;where recnsrelob  = '02' &#xA;and (ass.ASSIGNONLYINFO is null or ass.ASSIGNONLYINFO != 'X')&#xA;--and SMENR like '%80001'&#xA;--and vt.RECNNR like '2191810001001'&#xA;&#xA;go &#xA;&#xA;if object_id('dbo.cit_SollmieteVertrag') is not null drop table dbo.cit_SollmieteVertrag&#xA;go &#xA;select &#xA; vt.BUKRS, vt.SWENR, vt.SMENR, vt.meOBJNR, vt.meIMKEY, vt.SGENR, vt.SGRNR&#xA;, vt.xflaeche &#xA;, vt.xVertrag &#xA;,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,sol.CONDTYPE&#xA;, isnull(&#xA;&#x9;&#x9;BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sold.VALUEVALIDFROM,''), null),&#xA;&#x9;&#x9;BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDFROM,''), null)&#xA;&#x9;&#x9;)  as xBeginn&#xA;, case&#xA;  --  when BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sold.VALUEVALIDTO,''), null) &lt; case when vt.xvtEnde &lt;= getdate() and vt.xvtEnde &lt; BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null) &#xA;&#x9;&#x9;--&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   then vt.xvtEnde else sol.CONDVALIDTO end&#xA;&#x9;&#x9;--then sold.VALUEVALIDTO&#xA;&#x9;when vt.xvtEnde &lt;= getdate() and vt.xvtEnde &lt; BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null) &#xA;&#x9;&#x9;then vt.xvtEnde &#xA;&#x9;else BISDSYSDB.dbo.BIS_Convert_DATE&#xA;&#x9;(&#xA;&#x9;&#x9;isnull(&#xA;&#x9;&#x9;&#x9;&#x9;nullif(sold.VALUEVALIDTO,'')&#xA;&#x9;&#x9;&#x9;&#x9;, &#xA;&#x9;&#x9;&#x9;&#x9;nullif(sol.CONDVALIDTO,''))&#xA;&#x9;, null)&#xA;  end as xEnde  &#xA;, sol.UNITPRICE, sold.CONDVALUE, sold.TAXVALUE, isnull(sold.CONDCURR,sol.CONDCURR)  as CONDCURR, XCONDTYPEL, sol.CHGREASON, /*XCHGREASON, */ sol.CONDPURPOSEEXT&#xA;, cast(sol.CONDTYPE as varchar (10)) as xSollstellungsart &#xA;--&#x9;L = Lease &#xA;, TERMNOAJ = convert(varchar(10),ISNULL(sol.TERMNOAJ,'')) &#xA;, TERMNORH = convert(varchar(10),isnull(sol.TERMNORH,''))  &#xA;, ISNULL(flty.ssolhab,'S') as SSOLHAB&#xA;, CONDVALUEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sold.CONDVALUE) &#xA;, UNITPRICEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE)  &#xA;, CONDVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null) &#xA;, VALUEVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sold.VALUEVALIDTO,''), null)&#xA;, CONDGUID = convert(varchar,sol.CONDGUID,2)&#xA;, OBJNRX = sol.OBJNR&#xA;, vt.OBJVALIDFROMX, vt.OBJVALIDTOX &#xA;, vt.xPrefix&#xA;into dbo.cit_SollmieteVertrag&#xA;from&#x9;&#x9;dbo.cit_SAPVertrag vt with(nolock)&#xA;join&#x9;&#x9;dbo.SAP_VICDCOND sol on vt.INTRENOVT = sol.intreno and vt.MANDT = sol.MANDT&#xA;join&#x9;&#x9;dbo.SAP_VICDCONDDIST sold on sol.CONDGUID = sold.CONDGUID and sold.DISTOBJNR = vt.meOBJNR and sold.MANDT = vt.MANDT and sold.INTRENO = vt.INTRENOVT&#xA;join&#x9;&#x9;dbo.SAP_VIBDRO me on sold.DISTOBJNR = me.OBJNR and sold.MANDT = me.MANDT&#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPE cond on sol.CONDTYPE = cond.CONDTYPE and sol.MANDT = cond.MANDT&#xA;left join &#x9;dbo.SAP_TIVCDFLOWTYPE flty on cond.FLOWTYPE = flty.FLOWTYPE and cond.MANDT = flty.MANDT&#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPET solt on 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE and solt.MANDT = sol.MANDT&#xA;--left join &#x9;dbo.SAP_TIVCDCHGREAST  crt on sol.CONDTYPE = crt.CONDTYPE and  isnull(sol.CHGREASON,'') = crt.CHGREASON and 'D' = crt.SPRAS &#xA;where (sol.CONDDELETE is null or sol.CONDDELETE &lt;&gt; 'X')&#xA;--and xFlaeche = '2300_02075_001_20007'&#xA;--and sol.CONDTYPE = '100A'&#xA;order by xvertrag&#xA;--and (sol.CONDPURPOSEEXT is null or sol.CONDPURPOSEEXT &lt;&gt; 'I') &#xA;&#xA;&#xA;&#xA;go &#xA;delete t1 &#xA;  from dbo.cit_SollmieteVertrag t1&#xA;where xBeginn not between OBJVALIDFROMX and OBJVALIDTOX &#xA;go&#xA;if object_id('dbo.cit_SollmieteVertragADD') is not null drop table dbo.cit_SollmieteVertragADD&#xA;go &#xA;-- 2. Konditionen nicht in VICDCONDDIST nur aus CONDCALC &#xA;select &#xA; vt.BUKRS, vt.SWENR, vt.SMENR, vt.meOBJNR, vt.meIMKEY, vt.SGENR,vt.SGRNR &#xA;, vt.xflaeche &#xA;, vt.xVertrag &#xA;,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,sol.CONDTYPE&#xA;, isnull(BISDSYSDB.dbo.BIS_Convert_DATE(nullif(solc.VALUEVALIDFROM,''), null), BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDFROM,''), null)) as xBeginn&#xA;, case when vt.xvtEnde &lt;= getdate() &#xA;&#x9;and vt.xvtEnde &lt; BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDTO,''), null) &#xA;&#x9;then vt.xvtEnde else  isnull(BISDSYSDB.dbo.BIS_Convert_DATE(nullif(solc.VALUEVALIDTO,''), null) , BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDTO,''), null)) end as xEnde  &#xA;, sol.UNITPRICE, solc.CONDVALUE, solc.TAXVALUE, CONDCURR = isnull(solc.CONDCURR,sol.CONDCURR) , XCONDTYPEL, sol.CHGREASON, /*XCHGREASON, */ sol.CONDPURPOSEEXT&#xA;, cast(sol.CONDTYPE     &#x9;&#x9;as varchar (10))as xSollstellungsart&#xA;, sol.TERMNOAJ, sol.TERMNORH &#xA;, ISNULL(flty.ssolhab,'S') as SSOLHAB&#xA;, CONDVALUEX = bisdsysdb.dbo.BIS_Convert_FLOAT(solc.CONDVALUE) &#xA;, UNITPRICEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE)  &#xA;, CONDVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null) &#xA;, VALUEVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(solc.VALUEVALIDTO,''), null)&#xA;, OBJNRX = sol.OBJNR&#xA;, CONDGUID = convert(varchar,sol.CONDGUID ,2)&#xA;, vt.OBJVALIDFROMX, vt.OBJVALIDTOX &#xA;, vt.xPrefix&#xA;into &#x9;&#x9;dbo.CIT_SollmieteVertragAdd  &#xA;-- select top 100 vt.*&#xA;from&#x9;&#x9;dbo.cit_SAPVertrag vt with(nolock)&#xA;join&#x9;&#x9;dbo.SAP_VICDCOND sol on vt.INTRENOVT = sol.intreno&#xA;join&#x9;&#x9;dbo.SAP_VICDCONDCALC solc on sol.CONDGUID = solc.CONDGUID and vt.meOBJNR = sol.OBJNRDIST  &#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPE cond on sol.CONDTYPE = cond.CONDTYPE&#xA;left join &#x9;dbo.SAP_TIVCDFLOWTYPE flty on cond.FLOWTYPE = flty.FLOWTYPE&#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPET solt on 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE&#xA;--left join &#x9;dbo.SAP_TIVCDCHGREAST  crt on sol.CONDTYPE = crt.CONDTYPE and isnull(sol.CHGREASON,'') = crt.CHGREASON and 'D' = crt.SPRAS &#xA;where (sol.CONDDELETE is null or sol.CONDDELETE &lt;&gt; 'X')  &#xA;--and (sol.CONDPURPOSEEXT is null or sol.CONDPURPOSEEXT &lt;&gt; 'I')  &#xA;and not exists(select CONDGUID from&#x9;dbo.SAP_VICDCONDDIST dist where solc.CONDGUID = dist.CONDGUID)  &#xA;go &#xA;&#xA;delete t1 &#xA;  from dbo.CIT_SollmieteVertragAdd t1 &#xA;where xBeginn not between OBJVALIDFROMX and OBJVALIDTOX &#xA;go &#xA;insert into dbo.CIT_SollmieteVertrag &#xA;select * from dbo.CIT_SollmieteVertragAdd &#xA;go &#xA;if object_id('dbo.cit_SollmieteVertragADD') is not null drop table dbo.cit_SollmieteVertragADD&#xA;go &#xA;select &#xA; vt.BUKRS, vt.SWENR, vt.SMENR, vt.meOBJNR, vt.meIMKEY, vt.SGENR,vt.SGRNR &#xA;, vt.xflaeche &#xA;, vt.xVertrag &#xA;,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,sol.CONDTYPE&#xA;, BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDFROM,''), null)  as xBeginn&#xA;, case when vt.xvtEnde &lt;= getdate() &#xA;&#x9;and vt.xvtEnde &lt; BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null)  &#xA;&#x9;then vt.xvtEnde else BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDTO,''), null)  end as xEnde  &#xA;, sol.UNITPRICE, convert(varchar(512),null) as CONDVALUE, convert(varchar(512),null) as TAXVALUE, sol.CONDCURR, XCONDTYPEL, sol.CHGREASON, sol.CONDPURPOSEEXT&#xA;, cast(sol.CONDTYPE     &#x9;&#x9;as varchar (10))as xSollstellungsart &#xA;--&#x9;L = Lease &#xA;, TERMNOAJ = convert(varchar(10),ISNULL(sol.TERMNOAJ,'')) &#xA;, TERMNORH = convert(varchar(10),isnull(sol.TERMNORH,''))  &#xA;, ISNULL(flty.ssolhab,'S') as SSOLHAB&#xA;, CONDVALUEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE) &#xA;, UNITPRICEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE)  &#xA;, CONDVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO,''), null) &#xA;, VALUEVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(NULL, null)&#xA;, CONDGUID = convert(varchar,sol.CONDGUID ,2)&#xA;, OBJNRX = sol.OBJNR&#xA;, vt.OBJVALIDFROMX, vt.OBJVALIDTOX &#xA;, vt.xPrefix&#xA;into dbo.cit_SollmieteVertragADD &#xA;-- select top 100 vt.*&#xA;from&#x9;&#x9;dbo.cit_SAPVertrag vt with(nolock)&#xA;join&#x9;&#x9;dbo.SAP_VICDCOND sol on vt.INTRENOVT = sol.intreno and vt.meOBJNR = sol.OBJNRDIST &#xA;join&#x9;&#x9;dbo.SAP_VIBDRO me on sol.OBJNRDIST = me.OBJNR  &#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPE cond on sol.CONDTYPE = cond.CONDTYPE&#xA;left join &#x9;dbo.SAP_TIVCDFLOWTYPE flty on cond.FLOWTYPE = flty.FLOWTYPE&#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPET solt on 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE&#xA;--left join &#x9;dbo.SAP_TIVCDCHGREAST  crt on sol.CONDTYPE = crt.CONDTYPE and  isnull(sol.CHGREASON,'') = crt.CHGREASON and 'D' = crt.SPRAS &#xA;where (sol.CONDDELETE is null or sol.CONDDELETE &lt;&gt; 'X')  &#xA;--and (sol.CONDPURPOSEEXT is null or sol.CONDPURPOSEEXT &lt;&gt; 'I')  &#xA;--and sol.CHGREASON in('STAFFEL', 'INDEX')  &#xA;and not exists(select CONDGUID from dbo.SAP_VICDCONDDIST sold where sol.CONDGUID = sold.CONDGUID ) &#xA;and not exists(select CONDGUID from dbo.SAP_VICDCONDCALC calc where sol.CONDGUID = calc.CONDGUID )   &#xA;&#xA;go &#xA;-- Künftige Datensätze mit CHGREASON = FREIE oder NULL. Keine UNITPRICE = 0 mit abweichender Berechnungsvorschrift &#xA;insert into dbo.CIT_SollmieteVertragAdd  &#xA;select &#xA; vt.BUKRS, vt.SWENR, vt.SMENR, vt.meOBJNR, vt.meIMKEY, vt.SGENR,vt.SGRNR &#xA;, vt.xflaeche &#xA;, vt.xVertrag &#xA;,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,sol.CONDTYPE&#xA;, BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDFROM, ''), null)  as xBeginn&#xA;, case when vt.xvtEnde &lt;= getdate() &#xA;&#x9;and vt.xvtEnde &lt; BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO, ''), null)  &#xA;&#x9;then vt.xvtEnde else BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDTO, ''), null)  end as xEnde  &#xA;, sol.UNITPRICE, convert(varchar(512),null) as CONDVALUE, convert(varchar(512),null) as TAXVALUE, sol.CONDCURR, XCONDTYPEL, sol.CHGREASON, sol.CONDPURPOSEEXT&#xA;, cast(sol.CONDTYPE     &#x9;&#x9;as varchar (10))as xSollstellungsart &#xA;--&#x9;L = Lease &#xA;, TERMNOAJ = convert(varchar(10),ISNULL(sol.TERMNOAJ,'')) &#xA;, TERMNORH = convert(varchar(10),isnull(sol.TERMNORH,''))  &#xA;, ISNULL(flty.ssolhab,'S') as SSOLHAB&#xA;, CONDVALUEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE) &#xA;, UNITPRICEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE)  &#xA;, CONDVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(CONDVALIDTO, ''), null) &#xA;, VALUEVALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(NULL, null) &#xA;, CONDGUID = convert(varchar,sol.CONDGUID ,2)&#xA;, OBJNRX = sol.OBJNR&#xA;, vt.OBJVALIDFROMX, vt.OBJVALIDTOX &#xA;, vt.xPrefix&#xA;--into dbo.cit_SollmieteVertragADD &#xA;-- select top 100 vt.*&#xA;from&#x9;&#x9;dbo.cit_SAPVertrag vt with(nolock)&#xA;join&#x9;&#x9;dbo.SAP_VICDCOND sol on vt.INTRENOVT = sol.intreno and vt.meOBJNR = sol.OBJNRDIST &#xA;join&#x9;&#x9;dbo.SAP_VIBDRO me on sol.OBJNRDIST = me.OBJNR  &#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPE cond on sol.CONDTYPE = cond.CONDTYPE&#xA;left join &#x9;dbo.SAP_TIVCDFLOWTYPE flty on cond.FLOWTYPE = flty.FLOWTYPE&#xA;left join &#x9;dbo.SAP_TIVCDCONDTYPET solt on 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE&#xA;--left join &#x9;dbo.SAP_TIVCDCHGREAST  crt on sol.CONDTYPE = crt.CONDTYPE and  isnull(sol.CHGREASON,'') = crt.CHGREASON and 'D' = crt.SPRAS &#xA;where (sol.CONDDELETE is null or sol.CONDDELETE &lt;&gt; 'X')  &#xA;--and (sol.CONDPURPOSEEXT is null or sol.CONDPURPOSEEXT &lt;&gt; 'I')  &#xA;and (sol.CHGREASON = 'FREIE'  or sol.CHGREASON is NULL) &#xA;and sol.CALCRULE &lt;&gt; 'E1' &#xA;and BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDFROM, ''), null) &gt;= getdate() &#xA;and not exists(select CONDGUID from dbo.SAP_VICDCONDDIST sold where sol.CONDGUID = sold.CONDGUID ) &#xA;and not exists(select CONDGUID from dbo.SAP_VICDCONDCALC calc where sol.CONDGUID = calc.CONDGUID )   &#xA;&#xA;go  &#xA;delete t1 &#xA;  from dbo.CIT_SollmieteVertragAdd t1 &#xA;where xBeginn not between OBJVALIDFROMX and OBJVALIDTOX &#xA;go &#xA;delete &#xA;from dbo.cit_SollmieteVertragADD  &#xA;where xende &lt; xbeginn &#xA;go &#xA;delete t1 &#xA;from  dbo.cit_SollmieteVertragADD  t1 &#xA;join (select *, VALIDFROMX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(VALIDFROM,''), null) , VALIDTOX = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(VALIDTO,''), null) from SAP_VIBDRO) fl on fl.OBJNR = t1.meOBJNR &#xA;where (CHGREASON in('INDEX', 'FREIE') OR CHGREASON is null) and (t1.xbeginn &gt; fl.VALIDTOX or t1.xbeginn &lt;  VALIDFROMX) &#xA;go &#xA;delete mv &#xA;from dbo.cit_SollmieteVertragADD  mv &#xA;left join&#x9;(select vi.INTRENO, vi.NTREASON, NTRECEIPT = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTRECEIPT,''), null) &#xA;&#x9;&#x9;&#x9;&#x9; , vi.NTPARTY, NTACTIVATEDON ,  NTPER=BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTPER,''), null)&#xA;&#x9;&#x9;&#x9;&#x9; , NTAPPROVEDON = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTAPPROVEDON,''), null)&#xA;&#x9;&#x9;&#x9;&#x9; , NTREQUESTON  = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTREQUESTON,''), null)&#xA;&#x9;&#x9;&#x9;&#x9;from dbo.SAP_VICNNT vi &#xA;&#x9;&#x9;&#x9;&#x9;where NTCATE = 'C' and NTACTIVE = 'X' and (NTREVERSEDON is null or NTREVERSEDON  = '')) kuend on mv.INTRENOVT = kuend.INTRENO &#xA;left join   (select vi.INTRENO, OBJNR = ob.OBJNR , vi.NTREASON, NTRECEIPT = BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTRECEIPT,''), null)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, vi.NTPARTY, NTACTIVATEDON, NTPER=BISDSYSDB.dbo.BIS_Convert_DATE(nullif(NTPER,''), null)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;from&#x9;&#x9;&#x9;dbo.SAP_VICNNT vi  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;join&#x9;&#x9;&#x9;dbo.SAP_VICNNTOBJREL ob on vi.INTRENO = ob.INTRENO and vi.NTNO = ob.NTNO  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;where vi.NTCATE = 'O' and NTACTIVE = 'X' and (NTREVERSEDON is null or NTREVERSEDON = '00.00.0000')) kuendobj on mv.INTRENOVT = kuendobj.INTRENO and mv.meOBJNR = kuendobj.OBJNR &#xA;&#xA;where (CHGREASON in('INDEX', 'FREIE') OR CHGREASON is null)&#xA;and  (kuend.ntper &gt; xvtende or kuendobj.ntper &lt; xvtende ) &#xA;go &#xA;insert into dbo.cit_SollmieteVertrag (&#xA; BUKRS, SWENR, SMENR, meOBJNR, meIMKEY, SGENR, SGRNR , xFlaeche&#xA;, xVertrag,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,CONDTYPE, xBeginn, xEnde  &#xA;, UNITPRICE, CONDVALUE, TAXVALUE, CONDCURR, XCONDTYPEL, CHGREASON, CONDPURPOSEEXT&#xA;,xSollstellungsart , TERMNOAJ, TERMNORH, SSOLHAB, CONDVALUEX, UNITPRICEX, CONDVALIDTOX, VALUEVALIDTOX&#xA;,OBJVALIDFROMX, OBJVALIDTOX, xPrefix&#xA;) &#xA;select &#xA; BUKRS, SWENR, SMENR, meOBJNR, meIMKEY, SGENR, SGRNR , xFlaeche&#xA;, xVertrag,INTRENOVT, RECNNR, vtOBJNR, vtIMKEY, xvtBeginn, xvtEnde &#xA;,CONDTYPE, xBeginn, xEnde  &#xA;, UNITPRICE, CONDVALUE, TAXVALUE, CONDCURR, XCONDTYPEL, CHGREASON, CONDPURPOSEEXT&#xA;,xSollstellungsart , TERMNOAJ, TERMNORH, SSOLHAB, CONDVALUEX, UNITPRICEX, CONDVALIDTOX, VALUEVALIDTOX&#xA;,OBJVALIDFROMX, OBJVALIDTOX &#xA;, xPrefix&#xA;from&#x9;dbo.cit_SollmieteVertragADD &#xA;go &#xA;-- Konditionsende für künftige Verträge, falls Folgevertrag vorhanden &#xA;update a set xende = a.xvtende &#xA;from cit_sollmieteVertrag a with(nolock) &#xA;join cit_sollmieteVertrag b on a.meObjnr = b.meObjnr and a.xvtende &lt; b.xvtbeginn &#xA;where a.xvtende &gt; getdate() and a.xende &gt; a.xvtende &#xA;go &#xA;alter Table cit_sollmieteVertrag add &#xA;ciMonth varchar(12) null, CONDVALUEREFER varchar(10) null, FREQUENCYUNIT varchar(10) null, FREQUENCY varchar(10) null&#xA; &#xA;GO &#xA;update a set ciMonth = isnull(crh.ciMonth,'XXXXXXXXXXXX')&#xA;,  CONDVALUEREFER = rh.CONDVALUEREFER&#xA;,  FREQUENCYUNIT = rh.FREQUENCYUNIT &#xA;,  FREQUENCY = rh.FREQUENCY  &#xA;-- select *&#xA;from dbo.cit_SollmieteVertrag a &#xA;left join   dbo.SAP_VITMRH rh on INTRENOVT = rh.INTRENO and rh.termtype = '1110' and a.termnorh = isnull(rh.termno,'')  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;and xBeginn between BISDSYSDB.dbo.BIS_Convert_DATE(isnull(nullif(rh.validfrom,''),'1900-01-01'), null)  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and BISDSYSDB.dbo.BIS_Convert_DATE(isnull(nullif(rh.validto,''),'9999-12-31'), null)  &#xA;left join &#x9;dbo.cit_rymMonth crh on rh.frequencyunit = crh.frequencyunit and rh.frequency = crh.frequency  &#xA;and case when rh.Monthfrom = '13' then right(100+month(xbeginn),2) &#xA;&#x9; when rh.MonthFrom = '40' then &#xA;&#x9;&#x9;&#x9;&#x9;case when Rhythmbegin is not null then right(100+month(BISDSYSDB.dbo.BIS_Convert_DATE(nullif(Rhythmbegin,''), null)),2) &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else rh.Monthfrom end &#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;else rh.Monthfrom end = crh.Monthfrom &#xA;go &#xA;-- Umrechnen Betragsbasis Monatsbetrag und Rhythmus &lt;&gt; Monate &#xA;update a set CONDVALUEX = CONDVALUEX * 12.00 / convert(decimal(18,2),LEN(replace(cimonth,' ', '')))   &#xA;from dbo.cit_SollmieteVertrag a &#xA;where &#x9;CONDVALUEREFER = '0' &#xA;and not (FREQUENCYUNIT = '0' and FREQUENCY = '001') &#xA;and  LEN(replace(cimonth,' ', '')) &gt; 0 &#xA;and CONDVALUEX is not null &#xA;go  &#xA;-- Umrechnung Betragsbasis Jahresbetrag und Rhythmus &lt;&gt; Jahresrhythmus  &#xA;update a set CONDVALUEX = CONDVALUEX / convert(decimal(18,2),LEN(replace(cimonth,' ', '')))   &#xA;from dbo.cit_SollmieteVertrag a &#xA;where &#x9;CONDVALUEREFER = '1' &#xA;and FREQUENCYUNIT = '0' and FREQUENCY &lt;&gt; '012' &#xA;and  LEN(replace(cimonth,' ', '')) &gt; 0 &#xA;and CONDVALUEX is not null &#xA;go &#xA;-- Umrechnung Betragsbasis = Tagesbetrag &#xA;update a set CONDVALUEX = 30.00 * CONDVALUEX *12.00 /  convert(decimal(18,2),LEN(replace(cimonth,' ', '')))   &#xA;from dbo.cit_SollmieteVertrag a &#xA;where &#x9;CONDVALUEREFER = '2' &#xA;and  LEN(replace(cimonth,' ', '')) &gt; 0 &#xA;and CONDVALUEX is not null &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;GO &#xA;-- Ende für Einmalsollstellung eintragen sofern nicht vorhanden oder zu spät &#xA;if COL_LENGTH('dbo.cit_SollmieteVertrag', 'zzEnde') is null alter table cit_SollmieteVertrag add zzEnde datetime null &#xA;go &#xA;update a set zzEnde = dateadd(day,-1,dateadd(month,1,Dateadd(day,-day(xbeginn)+1,xbeginn)))  &#xA; from&#x9;cit_SollmieteVertrag a &#xA;where&#x9;CONDPURPOSEEXT = 'B' &#xA;and xEnde &gt; dateadd(day,-1,dateadd(month,1,Dateadd(day,-day(xbeginn)+1,xbeginn))) &#xA;go &#xA;update t1 set xende = t1.zzende &#xA;from&#x9;&#x9;dbo.cit_SollmieteVertrag t1 &#xA;left join&#x9;dbo.cit_SollmieteVertrag t2 on t1.xFlaeche = t2.xFlaeche and t1.xSollstellungsart = t2.xSollstellungsart and t2.xBeginn &gt; t1.xBeginn  &#xA;&#x9;and t2.xBeginn &lt;= t1.zzEnde  &#xA;where t1.zzende is not null &#xA;and t2.xBeginn is null &#xA;GO &#xA;-- Tagessollstlelung auf Monatsende verlängern sofern eintägig damit aktiviert &#xA;update t1 set xende = EOMONTH(xende)&#xA;from dbo.cit_SollmieteVertrag t1 &#xA;where CONDPURPOSEEXT = 'B' &#xA;and xbeginn = xende &#xA;&#x9;&#xA;-------------------------------------------------------------------------------------------------------------------------------------------------------------&#xA;-- TEXT Konditionen: Aufbereitung Leerstandsmiete Flächen &#xA;-------------------------------------------------------------------------------------------------------------------------------------------------------------&#xA;if OBJECT_ID('dbo.cit_SollmieteFlaeche') is not null drop table dbo.cit_SollmieteFlaeche &#xA;go  &#xA;Select  &#xA; me.BUKRS, me.SWENR, me.SMENR, me.OBJNR as meOBJNR, me.IMKEY as meIMKEY, me.SGENR, me.SGRNR, sol.INTRENO&#xA;--, cast(me.BUKRS + ' ' + right('000000' + me.SWENR,6) + ' ' + me.smenr as varchar (45))as xFlaeche&#xA;--, xvertrag&#x9;= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vt.RECNNR as varchar(50))&#xA;, xFlaeche&#x9;= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))&#xA;&#xA;,sol.CONDTYPE&#xA;, BISDSYSDB.dbo.BIS_Convert_DATE(isnull(nullif(solc.VALUEVALIDFROM,''), nullif(sol.CONDVALIDFROM,'')), null) as xBeginn&#xA;, BISDSYSDB.dbo.BIS_Convert_DATE(nullif(sol.CONDVALIDTO,''), null)  as xEnde  &#xA;, sol.UNITPRICE, solc.CONDVALUE, solc.TAXVALUE, solc.CONDCURR, XCONDTYPEL, sol.CHGREASON, sol.CONDPURPOSEEXT&#xA;, cast(sol.CONDTYPE     &#x9;&#x9;as varchar (10))as xSollstellungsart&#xA;, sol.TERMNOAJ, sol.TERMNORH &#xA;, ISNULL(flty.ssolhab,'S') as SSOLHAB&#xA;, CONDVALUEX = bisdsysdb.dbo.BIS_Convert_FLOAT(solc.CONDVALUE) &#xA;, UNITPRICEX = bisdsysdb.dbo.BIS_Convert_FLOAT(sol.UNITPRICE)  &#xA;, me.xPrefix&#xA;into dbo.cit_SollmieteFlaeche&#xA;from        dbo.SAP_VIBDRO me with(nolock)&#xA;join&#x9;&#x9;dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR &#xA;join&#x9;&#x9;dbo.SAP_VICDCOND     sol on me.INTRENO = sol.INTRENO&#xA;join&#x9;&#x9;dbo.SAP_VICDCONDCALC solc on sol.CONDGUID = solc.CONDGUID&#xA;left join&#x9;dbo.SAP_TIVCDCONDTYPE cond on sol.CONDTYPE = cond.CONDTYPE&#xA;left join  dbo.SAP_TIVCDFLOWTYPE flty on cond.FLOWTYPE = flty.FLOWTYPE&#xA;left join  dbo.SAP_TIVCDCONDTYPET solt on 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE&#xA;--left join  dbo.SAP_TIVCDCHGREAST  crt on sol.CONDTYPE = crt.CONDTYPE and sol.CHGREASON = crt.CHGREASON AND crt.SPRAS = 'D' &#xA;where (sol.CONDDELETE is null or sol.CONDDELETE &lt;&gt; 'X')   &#xA;and CONDPURPOSEEXT IN ('I', 'G') -- G=Leerstandsbuchung, I=Statistische Kondition&#xA;and obj.xobjnr is not null&#xA;&#xA;&#xA;go &#xA;alter Table cit_SollmieteFlaeche add ciMonth varchar(12) null &#xA;GO &#xA;update a set ciMonth = isnull(crh.ciMonth,'XXXXXXXXXXXX') &#xA;from cit_SollmieteFlaeche a &#xA;left join   dbo.SAP_VITMRH rh on a.INTRENO = rh.INTRENO and rh.termtype = '1110' and a.termnorh = isnull(rh.termno,'')  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;and xBeginn between BISDSYSDB.dbo.BIS_Convert_DATE(isnull(nullif(rh.validfrom,''),'1900-01-01'), null)  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and BISDSYSDB.dbo.BIS_Convert_DATE(isnull(nullif(rh.validto,''),'9999-12-31'), null) &#xA;left join &#x9;dbo.cit_rymMonth crh on rh.frequencyunit = crh.frequencyunit and rh.frequency = crh.frequency  &#xA;and case when rh.Monthfrom = '13' then right(100+month(xbeginn),2) &#xA;&#x9; when rh.MonthFrom = '40' then &#xA;&#x9;&#x9;&#x9;&#x9;case when Rhythmbegin is not null then right(100+month(BISDSYSDB.dbo.BIS_Convert_DATE(nullif(Rhythmbegin,''), null)),2) &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else rh.Monthfrom end &#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;else rh.Monthfrom end = crh.Monthfrom &#x9;&#x9;&#xA;go &#xA;&#xA;&#xA;-- ungültige Mietkonditionen entfernen&#xA;delete&#xA;-- select *&#xA;from cit_SollmieteFlaeche &#xA;where xBeginn &gt; xEnde&#xA;and xEnde is not null&#xA;&#xA;delete &#xA;-- select * &#xA;from cit_SollmieteVertrag&#xA;where xBeginn &gt; xEnde&#xA;and xEnde is not null" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
