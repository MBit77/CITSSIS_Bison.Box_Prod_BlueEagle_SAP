set xvertrNaechstesVertragsende = case when isnull(xvtEnde, '99991231') &lt; getdate() then xvtEnde else BISDSYSDB.dbo.BIS_Convert_DATE(rnendnew, null) end
from " + @[User::cfgINTDB] + ".dbo.xinpVertrag inp
join
(
	select rnendnew, 
	xVertrag = obj.xobjnr +' '+ right(isnull(me.SGENR, 'G'+me.SGRNR), 3) + ' ' + right(me.SMENR,5) + ' ' + vtg.RECNNR
	from	SAP_vicncn vtg
	join
	(
		select xHubid, intreno, rnendnew
		, r = ROW_NUMBER() over (partition by INTRENO order by " + @[User::cfgSDB] + ".dbo.BIS_CONVERT_DATE(rnactivatedon,null)  desc) -- Gruppiert nach Vertrag und absteigende Sortiertung nach Datum der Inanspruchnahme
		from dbo.SAP_VICNRN 
		where rnactive = 'X' -- Option in Anspruch genommen
	) nxtEnd on vtg.xHubid = nxtEnd.xHubId and vtg.INTRENO = nxtEnd.intreno
	join dbo.SAP_VIBDOBJASS ass on vtg.xHubId = ass.xHubId and  '10' = OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
	join dbo.SAP_VIBDRO me on vtg.xHubId = me.xHubId and ass.OBJNRTRG = me.OBJNR
	join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
	where obj.xobjnr is not null 
	and nxtEnd.r = 1
) x on inp.xVertrag = x.xVertrag


