              <DTS:VariableValue
                DTS:DataType="8">select distinct
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= '0'
, xggObjekt			= cast(obj.xobjnr as varchar(50))
, xGrundGebaeude	= cast(obj.xobjnr +'_'+ right(geb.SGENR,3) as varchar(50))
, xggMatchcode		= cast(geb.XGETXT as varchar(150))
, xggBezeichnung	= cast(isnull(rtrim(adr.POST_CODE1 + ' ' + adr.CITY1), geb.XGETXT) as varchar(150))
, xggArt			= '0'
, xggBaujahr		= left(nullif(geb.YBAUJAHR, ''), 4)
, xggPostleitzahl	= adr.POST_CODE1
, xggStrasse		= isnull(adr.STREET, case when charindex('/', geb.XGETXT) &gt; 1 then substring(geb.XGETXT, charindex('/', geb.XGETXT)+2, 100) end)
, xggOrt			= isnull(adr.CITY1, case when (charindex('/', geb.XGETXT)&gt;0) then rtrim(isnull(adr.CITY1, left(geb.XGETXT, charindex('/', geb.XGETXT)-1))) end)
, xggGesellsch		= obj.xGesellNr
, xggBeginn			= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(geb.VALIDFROM,''), nullif(obj.VALIDFROM,''))), NULL)
, xggEnde			= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(geb.VALIDTO, '99991231'), nullif(obj.VALIDFROM, '99991231'))), NULL)
, xggEnde_intf		= '1'
, xgghnr			= adr.HOUSE_NUM1
-- select obj.*
from		dbo.SAP_vibdbu geb
join		dbo.SAP_vibdbe obj on geb.xHubid = obj.xHubId and geb.BUKRS = obj.BUKRS and geb.SWENR = obj.SWENR
left join	dbo.SAP_VZOBJECT vz on geb.xHubId = vz.xHubId and vz.ADROBJNR = geb.INTRENO and vz.ADROBJTYP = 'VI'
left join	dbo.SAP_Adrc adr on geb.xHubId = adr.xHubId and vz.ADRNR = adr.ADDRNUMBER
where obj.xobjnr is not null

union

select distinct
  dtedit			= getdate()
, luserkz			= 'INTERFACE'
, ldelete			= '0'
, xggObjekt			= cast(obj.xobjnr as varchar(50))
, xGrundGebaeude	= cast(obj.xobjnr +'_'+ 'G'+ right(grund.SGRNR,3) as varchar(50))
, xggMatchcode		= cast(grund.XGRTXT as varchar(150))
, xggBezeichnung	= cast(isnull(rtrim(adr.POST_CODE1 + ' ' + adr.CITY1), grund.XGRTXT) as varchar(150))
, xggArt			= '1'
, xggBaujahr		= null
, xggPostleitzahl	= adr.POST_CODE1
, xggStrasse		= isnull(adr.STREET, case when charindex('/', grund.XGRTXT) &gt; 1 then substring(grund.XGRTXT, charindex('/', grund.XGRTXT)+2, 100) end)
, xggOrt			= rtrim(isnull(adr.CITY1, case when charindex('/', grund.XGRTXT) &gt; 1 then left(grund.XGRTXT, charindex('/', grund.XGRTXT)-1) end))
, xggGesellsch		= obj.xGesellNr
, xggBeginn			= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(grund.VALIDFROM,''), nullif(obj.VALIDFROM,''))), NULL)
, xggEnde			= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), isnull(nullif(grund.VALIDTO, '99991231'), nullif(obj.VALIDTO, '99991231'))), NULL)
, xggEnde_intf		= '1'
, xgghnr			= adr.HOUSE_NUM1
-- select *
from		dbo.SAP_VIBDPR grund
join		dbo.SAP_vibdbe obj	on grund.xHubid = obj.xHubId and grund.BUKRS = obj.BUKRS and grund.SWENR = obj.SWENR
left join	dbo.SAP_VZOBJECT vz on grund.xHubid = vz.xHubid and vz.ADROBJNR = grund.INTRENO and vz.ADROBJTYP = 'VI'
left join	dbo.SAP_Adrc adr	on grund.xHubid = adr.xHubid and vz.ADRNR = adr.ADDRNUMBER
where obj.xobjnr is not null
