                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
  dtedit		= getdate()
, luserkz		= 'INTERFACE'
, ldelete		= '0'
/* RS - 20230426: wenn mehrere Verträge zu einem Hauptvertrag, dann erzeuge eine laufende Nummer */
--, xVertrag	= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR as varchar(50))
  , xVertrag	= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR + case when assAnz.lid is null or NbrVt = '' then '' else '_' + right('00'+ass.NbrVt,2) end as varchar(100))  
, xkaFlaeche	= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xvtObjekt		= cast(obj.xobjnr as varchar(50))
, xkaSoll		= try_convert(decimal (18, 2), kaut.[0000000031])
, xkauthaben	= try_convert(decimal (18, 2), kaut.[0000000031])
, xkaWaehrung	= vtg.RECNCNCURR
, xkaArt		= obj.xPrefix +'_'+ kaut.[0000000032]
from 
(
	select *
	-- vorher nur auf die benötigten Spalten einschränken, sonst entstehen in pivot doppelte Zeilen
	from (select ad_key, valid_from, ad_data, var_id from SAP_VIAD_DATA_KAUTIONEN ) unPvt
	pivot (max(ad_data) for var_id in ([0000000031], [0000000032])) pvt
) kaut
join SAP_VICNCN vtg on vtg.INTRENO = kaut.AD_KEY
JOIN
(	
    SELECT xHubID, MANDT, OBJNRSRC, OBJNRTRG, ASSIGNONLYINFO
        , NbrVt = isnull(nullif(convert(varchar(10),ROW_NUMBER() over(partition by OBJNRSRC, OBJNRTRG order by isnull(validTo,'99991231'))),'1'),'')   
    FROM dbo.SAP_VIBDOBJASS
	WHERE OBJASSTYPE = '10'
) ass ON vtg.xHubid = ass.xHubid and vtg.OBJNR = ass.OBJNRSRC
LEFT JOIN
(
SELECT lid = 1, OBJNRSRC, OBJNRTRG, xhubid, MANDT, Anzahl = count(OBJASSTYPE)
FROM dbo.SAP_VIBDOBJASS WHERE OBJASSTYPE = '10'
GROUP BY OBJNRSRC, OBJNRTRG, xhubid, MANDT
) assAnz on assAnz.OBJNRSRC = ass.OBJNRSRC and assAnz.OBJNRTRG = ass.OBJNRTRG and assAnz.xHubID = ass.xHubID and ass.MANDT = assAnz.MANDT and assAnz.Anzahl &gt; 1
join SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
join SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
where obj.xObjNr is not null
-- Diese Kautionsarten stehen in SAP über eine Dropdown - Auswahl zur Verfügung
and [0000000032]
in
(
'keine vereinbart',
'BKXL-Kaution',
'Cashkonto D&amp;R',
'Barkaution',
'Bankbürgschaft',
'Escrow',
'Konzernbürgschaft',
'Sparbuch/Verpfändung',
'Patronatserklärung',
'Liegt noch nicht vor'
)</property>
