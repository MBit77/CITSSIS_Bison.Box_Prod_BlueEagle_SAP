                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
  dtedit					= getdate()
, luserkz					= 'INTERFACE'
, ldelete					= '0'
, xVertrag					= cast(obj.xobjnr +' '+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) + ' ' + right(me.SMENR,5) + ' ' + vtg.RECNNR as varchar(50))
, xFlaeche					= cast(obj.xobjnr +' '+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) + ' ' + right(me.SMENR,5) as varchar(50))
, xIndexnr					= obj.xPrefix +'_'+ INDEXSERIES + isnull(rtrim(INDEXBASEYEAR), '')
, xBasisjahr				= INDEXBASEYEAR
, xviErstepruefung			= BISDSYSDB.dbo.BIS_Convert_DATE(COALESCE(nullif(nullif(ADJMFIRSTDATE,''), '00000000'), nullif(nullif(idxAnpassung.FIRSTINDEXDATE,''), '00000000'), nullif(nullif(vtg.RECNBEG, ''), '00000000'),'19000101'), NULL)
--, xviErstepruefung			= ix.IndexErstePrf
, xviSchwelle				= case when BISDSYSDB.dbo.BIS_Convert_DEC(INDEXCHGPERCENT) &gt; 0 then BISDSYSDB.dbo.BIS_Convert_DEC(INDEXCHGPERCENT) else BISDSYSDB.dbo.BIS_Convert_DEC(INDEXCHGPOINTS) end
, xviPruefungsart			= case when BISDSYSDB.dbo.BIS_Convert_DEC(INDEXCHGPERCENT) &gt; 0 then 'Prozent' else  'Punkte' end
, xviPrueffrequenz			= case BISDSYSDB.dbo.BIS_Convert_DEC(RHYTHMUNIT)
								when 0 then 1
								when 1 then 4
								when 2 then 12 
								else 1 
							  end 
							 * case when RHYTHM = 0 then 1 else BISDSYSDB.dbo.BIS_Convert_DEC(RHYTHM) end
, xviNPmon					= case BISDSYSDB.dbo.BIS_Convert_DEC(DELAYBASEUNIT)
								when 0 then 1
								when 1 then 4
								when 2 then 12 
								else 1 
							 end * BISDSYSDB.dbo.BIS_Convert_DEC(DELAYBASE)
, xviWeitergabe				= BISDSYSDB.dbo.BIS_Convert_DEC(PERCENTAGEPASS)
, xvertridxDatumLetzteAnp	= BISDSYSDB.dbo.BIS_Convert_DATE(idxAnpassung.ADJMINDEXDATE_max, NULL)
, xvertridxErsteanp			= BISDSYSDB.dbo.BIS_Convert_DATE(idxAnpassung.ADJMINDEXDATE_min, NULL)
, xvertridxStandLetzteAnp	= BISDSYSDB.dbo.BIS_Convert_DEC(idxAnpassung.ADJMINDEXBASE)
-- select *
 from dbo.SAP_vicncn vtg with(nolock)
 join dbo.SAP_vibdobjass ass on vtg.xHubid = ass.xHubid and '10' = ass.OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
 join dbo.SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
 join dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR join dbo.SAP_TIV26 t26 on vtg.RECNTYPE = t26.SMVART
 join dbo.citSAP_Indexvereinbarung ix on vtg.INTRENO = ix.INTRENO
 left join 
 (
	 select 
	  INTRENO
	, ADJMINDEXDATE_max = ADJMINDEXDATE
	, ADJMINDEXDATE_min = min(ADJMINDEXDATE)	over (partition by INTRENO)
	, FIRSTINDEXDATE	= min(FIRSTINDEXDATE)	over (partition by INTRENO)
	, ADJMINDEXBASE
	, r=ROW_NUMBER() over (partition by INTRENO order by ADJMINDEXDATE DESC)
	from BISimportDB.dbo.SAP_VIAJRECINDX 
 ) idxAnpassung on idxAnpassung.INTRENO = ix.INTRENO and idxAnpassung.r = 1 
where INDEXSERIES is not null
 -- nur vermietungsverträge
 and t26.RECNSRELOB = 2
 and isnull(ass.ASSIGNONLYINFO, '') &lt;&gt; 'X'</property>
