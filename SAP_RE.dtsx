-- ME's auf dem Gebäude
select distinct
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
, xflGrundGebaeude		= cast(obj.xobjnr +'_'+ right(me.SGENR, 3) as varchar(50))
, xFlaeche				= cast(obj.xobjnr +'_'+ right(me.SGENR, 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xflMatchcode			= cast(me.XMETXT as varchar(100))
-- Adresse
--, xflMatchcode			= cast(rtrim(isnull(adr.STREET, substring(geb.XGETXT, 
--																case when charindex('/ ', geb.XGETXT) &gt; 1 then charindex('/ ', geb.XGETXT)+2
--																	 when charindex(', ', geb.XGETXT) &gt; 1 then charindex(', ', geb.XGETXT)+2 
--																	 else 1 
--																end
--															, 100))) + isnull(' '+ rtrim(adr.HOUSE_NUM1), '') as varchar(100))
-- Lage + Etage
, xflBezeichnung		= cast(case 
							when nullif(me.RLGESCH, '') is not null then me.RLGESCH + isnull(' '+me.SSTOCKW, '')
							else na.XMBEZ
						  end as varchar(150))
, xflFlaechenart		= obj.xPrefix  +'_'+ me.SNUNR
, xflZugangsdatum		= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), COALESCE(nullif(me.VALIDFROM,''), nullif(geb.VALIDFROM,''), nullif(obj.VALIDFROM,''), '19000101')), NULL)
, xflAbgangsdatum		= BISDSYSDB.dbo.BIS_Convert_DATE(COALESCE(nullif(me.VALIDTO, '99991231'), nullif(geb.VALIDTO, '99991231'), nullif(obj.VALIDTO, '99991231')), NULL)
, xflAbgangsdatum_intf	= '1'
, xflLage				= convert(varchar(50), t3h.XMLGESCH)
, xflEtage				= convert(varchar(20), t3g.XSTOCKM)
-- select RLGESCH, SSTOCKW, me.*
from		dbo.SAP_VIBDRO me
join		dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
join		dbo.SAP_VIBDBU geb on me.xHubid = geb.xHubid and me.BUKRS = geb.BUKRS and me.SWENR = geb.SWENR and me.SGENR = geb.SGENR
join		dbo.SAP_TIV0A na on me.xHubid = na.xHubid and me.SNUNR = na.SNUNR and na.SPRAS = 'D'
left join	dbo.SAP_VZOBJECT vz on me.xHubid = vz.xHubid and vz.ADROBJNR = geb.INTRENO and vz.ADROBJTYP = 'VI'
left join	dbo.SAP_Adrc adr on me.xHubid = adr.xHubid and vz.ADRNR = adr.ADDRNUMBER
left join dbo.SAP_TIV3H t3h on me.xHubID = t3h.xHubID and me.RLGESCH = t3h.RLGESCH and t3h.SPRAS = 'D'
left join dbo.SAP_TIV3G t3g on me.xHubID = t3g.xHubID and me.SSTOCKW = t3g.SSTOCKW and t3g.SPRAS = 'D'

where me.SGENR is not null 
and obj.xobjnr is not null 

union

-- ME's auf dem Grundstück
select distinct 
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
, xflGrundGebaeude		= cast(obj.xobjnr +'_'+ 'G'+ right(me.SGRNR, 3) as varchar(50))
, xFlaeche				= cast(obj.xobjnr +'_'+ 'G'+ right(me.SGRNR, 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xflMatchcode			= cast(me.XMETXT as varchar(100))
-- Adresse
-- Bei Grundstücksflächen wird die Adresse in XGRTXT im Format (Ort, Str Hnr) gespeichert, der Ort wird abgeschnitten
--, xflMatchcode			= cast(rtrim(isnull(adr.STREET, substring(grund.XGRTXT, 
--																case when charindex('/ ', grund.XGRTXT) &gt; 1 then charindex('/ ', grund.XGRTXT)+2
--																	 when charindex(', ', grund.XGRTXT) &gt; 1 then charindex(', ', grund.XGRTXT)+2 
--																	 else 1 
--																end
--															, 100))) + isnull(' '+ rtrim(adr.HOUSE_NUM1), '') as varchar(100))

-- Lage + Etage
, xflBezeichnung		= cast(case 
							when nullif(me.RLGESCH, '') is not null then me.RLGESCH + isnull(' '+me.SSTOCKW, '')
							else na.XMBEZ
						  end as varchar(150))
, xflFlaechenart		= obj.xPrefix +'_'+ me.SNUNR
, xflZugangsdatum		= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(8000), COALESCE(nullif(me.VALIDFROM,''), nullif(grund.VALIDFROM,''), nullif(obj.VALIDFROM,''), '19000101')), NULL)
, xflAbgangsdatum		= BISDSYSDB.dbo.BIS_Convert_DATE(COALESCE(nullif(me.VALIDTO, '99991231'), nullif(grund.VALIDTO, '99991231'), nullif(obj.VALIDTO, '99991231')), null)
, xflAbgangsdatum_intf	= '1'
, xflLage				= convert(varchar(50), t3h.XMLGESCH)
, xflEtage				= convert(varchar(20), t3g.XSTOCKM)
-- select me.*
from		dbo.SAP_VIBDRO me
join		dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
join		dbo.SAP_VIBDPR grund on me.xHubid = grund.xHubid and me.BUKRS = grund.BUKRS and me.SWENR = grund.SWENR and me.SGRNR = grund.SGRNR
join		dbo.SAP_TIV0A na on me.xHubid = na.xHubid and me.SNUNR = na.SNUNR and na.SPRAS = 'D'
left join	dbo.SAP_VZOBJECT vz on me.xHubid = vz.xHubid and vz.ADROBJNR = grund.INTRENO and vz.ADROBJTYP = 'VI'
left join	dbo.SAP_Adrc adr on me.xHubid = adr.xHubid and vz.ADRNR = adr.ADDRNUMBER
left join dbo.SAP_TIV3H t3h on me.xHubID = t3h.xHubID and me.RLGESCH = t3h.RLGESCH and t3h.SPRAS = 'D'
left join dbo.SAP_TIV3G t3g on me.xHubID = t3g.xHubID and me.SSTOCKW = t3g.SSTOCKW and t3g.SPRAS = 'D'
where me.SGRNR is not null
and obj.xobjnr is not null
</DTS:VariableValue>
