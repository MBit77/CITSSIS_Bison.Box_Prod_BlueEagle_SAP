              <DTS:VariableValue
                DTS:DataType="8">select distinct 
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
, xFlaeche				= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xSollstellungsart		= me.xPrefix + '_' + sol.CONDTYPE
, xBeginn				= BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(500), sol.CONDVALIDFROM), NULL) 
, xsmMonate				= CONVERT(CHAR(12),REPLACE(isnull(crh.ciMonth,'XXXXXXXXXXXX'),0xA0,0x20))
, xsmWert				= BISDSYSDB.dbo.BIS_Convert_DEC(isnull(solCalc.CONDVALUE, sol.UNITPRICE)) * Case when SSOLHAB = 'H' THEN -1 else 1 end
, xsmWaehrung			= isnull(sol.CONDCURR, 'EUR')
, xsollstvtgref			= cast(obj.xobjnr +' '+ right(isnull(me.SGENR, 'G'+me.SGRNR), 3) + ' ' + right(me.SMENR,5) + ' ' + vtg.RECNNR as varchar(50))
-- select sol.*, rh.Monthfrom
from		dbo.SAP_vicncn vtg with(nolock)
join        dbo.SAP_vibdobjass ass on vtg.xHubid = ass.xHubid and '10' = ass.OBJASSTYPE and vtg.OBJNR = ass.OBJNRSRC
join        dbo.SAP_vibdro me  with(nolock) on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
join		dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRs = obj.BUKRS and me.SWENR = obj.SWENR 
join		dbo.SAP_VICDCOND sol on me.xHubid = sol.xHubid and me.OBJNR = sol.OBJNR and vtg.INTRENO = sol.INTRENO
LEFT join	dbo.SAP_VICDCONDCALC solCalc on sol.xHubid = solCalc.xHubid and sol.CONDGUID = solCalc.CONDGUID and vtg.INTRENO = solCalc.INTRENO
left join dbo.SAP_TIVCDCONDTYPE cond on sol.xHubid = cond.xHubid and  sol.CONDTYPE = cond.CONDTYPE
left join dbo.SAP_TIVCDFLOWTYPE flty on sol.xHubid = flty.xHubid and cond.FLOWTYPE = flty.FLOWTYPE
left join dbo.SAP_TIVCDCONDTYPET solt on sol.xHubid = solt.xHubid and 'D' = solt.SPRAS and sol.CONDTYPE = solt.CONDTYPE
-- Zahlungsfrequenz
left join dbo.SAP_VITMRH rh on sol.xHubid = solt.xHubid and sol.INTRENO = rh.INTRENO and rh.termtype = '1110' and isnull(sol.TERMNORH, '') = isnull(rh.TERMNO, '')
--	and BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(500), sol.CONDVALIDFROM), NULL) between BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(500), rh.validfrom), NULL) and BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(500), rh.validto), NULL)
left join dbo.cit_rymMonth crh on rh.frequencyunit = crh.frequencyunit and rh.frequency = crh.frequency
	and crh.Monthfrom = 
	case when rh.Monthfrom = '13' then right(100 + month(BISDSYSDB.dbo.BIS_Convert_DATE(convert(char(500), sol.CONDVALIDFROM), NULL)),2) 
		 when rh.MonthFrom = '40' then 
					case when Rhythmbegin is not null then right(100+month(convert(datetime,Rhythmbegin,120)),2) 
						else rh.Monthfrom 
					end 			
		else rh.Monthfrom 
	end
where isnull(sol.CONDDELETE, '') != 'X'
and left(sol.INTRENO, 2) = 'IS' -- IS=Vertragskonditionen, IM=Marktmieten/Leerstandsmieten
and obj.xobjnr is not null
</DTS:VariableValue>
