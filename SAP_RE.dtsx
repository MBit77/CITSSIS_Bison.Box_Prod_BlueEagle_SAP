      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2105E5C9-C9EB-4DDA-8714-2A9812EBB32F}"
          SQLTask:SqlStatementSource="-----------------------------------------------------------------------------&#xA;-- Sollmieten aus Konditionen Mietobjekt&#xA;-----------------------------------------------------------------------------&#xA;insert into bisintdata.dbo.xinpSollmiete (dtedit,luserkz, ldelete, xSollstellungsart,xFlaeche,xBeginn,xEnd,xsmMonate,xsmWert,xsmWaehrung,xrecordnr,xsmSollstGrund)&#xA;select distinct&#xA;   GetDate()                                                        as dtedit&#xA; , cast('Interface'                                 as varchar (45))as luserkz&#xA; , ldelete = '0'   &#xA; , cast(xPrefix + '_' + a.xSollstellungsart + '_RO' as varchar (45))as xSollstellungsart&#xA; , cast(a.xFlaeche                                  as varchar (45))as xFlaeche&#xA; , a.xBeginn                                                        as xBeginn&#xA; , a.xEnde&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;as xEnd&#xA; , cast(ciMonth                                     as char (12)) as xsmMonate&#xA; , cast(isnull(a.CONDVALUEX, UNITPRICEX) &#xA;   * Case when SSOLHAB = 'H' &#xA;     THEN -1 else 1 &#xA;     end         as dec (15,4)) as xsmWert&#xA; , cast(a.CONDCURR                                  as varchar (45))as xsmWaehrung&#xA; , cast(1000000000                                  as dec (21,0)) as xrecordnr&#xA; , cast(case when nullif(a.CHGREASON,'') is not null then&#xA;         a.CHGREASON&#xA;         else '000' end                     as varchar(45)) as xsmSollstGrund&#xA;from dbo.cit_SollmieteFlaeche a&#xA;&#xA;UNION&#xA;&#xA;-- 0 Sätze bei Vertragsende&#xA;select distinct&#xA;   GetDate()              as dtedit&#xA; , cast('Interface'         as varchar (45))as luserkz&#xA; , ldelete = '0'   &#xA; , cast(xPrefix + '_' + a.xSollstellungsart + '_RO' as varchar (45))as xSollstellungsart&#xA; , cast(a.xFlaeche         as varchar (45))as xFlaeche&#xA; , dateadd(d,1,a.xEnde)            as xBeginn&#xA; , a.xEnde&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   as xEnd&#xA; , cast(ciMonth          as char    (12))as xsmMonate&#xA; , cast(0.00          as dec   (15,4))as xsmWert&#xA; , cast(a.CONDCURR         as varchar (45))as xsmWaehrung&#xA; , cast(1000000000         as dec   (21,0))as xrecordnr&#xA; , cast(case when nullif(a.CHGREASON,'') is not null then&#xA;         a.CHGREASON&#xA;         else '000' end             as varchar(45)) as xsmSollstGrund&#xA;from dbo.cit_SollmieteFlaeche a&#xA;left join  (select xflaeche, xSollstellungsart, xbeginn, xende from cit_SollmieteFlaeche) b on a.xflaeche = b.xflaeche&#xA;                                                                         and a.xSollstellungsart = b.xSollstellungsart&#xA;                                                                         and dateadd(d,1,a.xEnde) = b.xBeginn&#xA;where a.xEnde &lt; convert(datetime,'99991231',112) and b.xBeginn is null&#xA;and '0' = '1'&#xA;&#xA;-----------------------------------------------------------------------------&#xA;-- Vertragsende in vorbereitete Sollstellungen eintragen&#xA;-----------------------------------------------------------------------------&#xA;&#xA;--update sol set xvtende = vt.xvtEnde, xEnde = case when xEnde &gt; vt.xvtEnde then vt.xvtEnde else xEnde end  &#xA;--from BISIMPORTDB.dbo.cit_SollmieteVertrag sol &#xA;--join bisintdata.dbo.xinpVertrag vt on sol.xVertrag  = vt.xVertrag &#xA;&#xA;-----------------------------------------------------------------------------&#xA;-- Sollmiete aus Vertrag&#xA;-----------------------------------------------------------------------------&#xA;&#xA;if object_id('tempdb.dbo.#Sollmiete') is not null drop table dbo.#Sollmiete &#xA;if object_id('tempdb.dbo.#SollmieteSumme') is not null drop table dbo.#SollmieteSumme&#xA;&#xA;select &#xA;a.xPrefix + '_' + a.xSollstellungsart + case when LEFT(OBJNRX,2) = 'IM' then '_X' else '' end + '_CN' as xSollstellungsart&#xA;, xFlaeche  = a.xFlaeche&#xA;, xBeginn  = a.xbeginn&#xA;, xEnde    = a.xEnde&#xA;, xsmMonate  = convert(char(12), case when rh.FIXPER in (1,2) then 'X  X  X  X  ' else ciMonth end) &#xA;, xsmWert  = convert(decimal(15,4), isnull(a.CONDVALUEX,UNITPRICEX) * Case when SSOLHAB = 'H' THEN -1 else 1 end)   &#xA;, xsmSollstGrund= convert(varchar(45),case when nullif(a.CHGREASON,'') is not null then a.CHGREASON else '000' end)  &#xA;, xsollstOnce = convert(char(1),  case when CONDPURPOSEEXT = 'B' then '1' end) &#xA;, xsmWaehrung   = convert(varchar(50),a.CONDCURR) &#xA;into dbo.#Sollmiete &#xA;from  dbo.cit_SollmieteVertrag a&#xA;left join (select distinct INTRENO, FIXPER, TERMNO, r=ROW_NUMBER() over (partition by INTRENO, TERMNO order by VALIDFROM desc) from SAP_VITMRH) rh on a.INTRENOVT = rh.INTRENO and a.TERMNORH = rh.TERMNO and rh.r = 1&#xA;where xvtende is null or xvtende is not null and xbeginn &lt; xvtende&#xA;&#xA;&#xA;select * into dbo.#SollmieteSumme from ( &#xA;select xSollstellungsart, xFlaeche, xBeginn, xEnde, xsmMonate, xsmWaehrung, xsmWert = sum(xsmwert) &#xA;from  dbo.#Sollmiete &#xA;group by xSollstellungsart, xFlaeche, xBeginn, xEnde, xsmMonate, xsmWaehrung &#xA;union &#xA;select xSollstellungsart, xFlaeche, xBeginn = xEnde + 1, xEnde, xsmMonate, xsmWaehrung, xsmWert = sum(-1.00 * xsmwert) &#xA;from  dbo.#Sollmiete &#xA;where xEnde &lt; convert(datetime,'99991231',112) &#xA;and '0' = '1'&#xA;group by xSollstellungsart, xFlaeche, xende+1,xEnde, xsmMonate, xsmWaehrung &#xA;) s &#xA;&#xA;&#xA;;with Sollst0 as ( &#xA;select *, Betrag = sum(xsmWert)over(partition by xSollstellungsart, xFlaeche,xsmMonate,xsmWaehrung order by xBeginn rows between UNBOUNDED PRECEDING and current row) &#xA;, zr = Row_Number() over (partition by  xSollstellungsart, xFlaeche,xsmMonate,xsmWaehrung,xBeginn order by xBeginn) &#xA;from dbo.#SollmieteSumme &#xA;) &#xA;, Sollstellung as (select * , xr=ROW_NUMBER() over(partition by xSollstellungsart, xFlaeche,xsmMonate,xsmWaehrung,xBeginn,xEnde order by zr desc) &#xA;from Sollst0 ) &#xA;insert into bisintdata.dbo.xinpSollmiete ( dtedit,luserkz,ldelete&#xA; ,xSollstellungsart, xFlaeche,xBeginn,xEnd,xsmMonate,xsmWert,xsmWaehrung,xrecordnr)&#xA;select &#xA;dtedit = getdate(), luserkz = 'interface', ldelete = 0 &#xA; , xSollstellungsart,xFlaeche,xBeginn,xEnde,xsmMonate,xsmWert = case when '0' = '1' then Betrag else xsmWert end,xsmWaehrung,xrecordnr = convert(int,1000000000)&#xA; from  Sollstellung t1&#xA; where xr=1&#xA;&#xA;&#xA;&#xA;update t1 set  xsmSollstGrund = t2.xsmSollstGrund, xsollstOnce = t2.xsollstOnce &#xA;from BISINTDATA.dbo.xinpSollmiete t1 &#xA;join dbo.#Sollmiete t2 on t1.xFlaeche = t2.xFlaeche and t1.xBeginn = t2.xBeginn and t1.xSollstellungsart = t2.xSollstellungsart &#xA;&#xA;&#xA;update t1 set  xsmSollstGrund = t2.xsmSollstGrund, xsollstOnce = t2.xsollstOnce &#xA;from BISINTDATA.dbo.xinpSollmiete t1  &#xA;join dbo.#Sollmiete t2 on t1.xFlaeche = t2.xFlaeche and t1.xBeginn = t2.xende + 1 and t1.xSollstellungsart = t2.xSollstellungsart &#xA;where t2.xEnde &lt; convert(datetime,'99991231',112) &#xA;and t1.xsmSollstgrund is null &#xA;&#xA;-- Doppler bei Rhythmuswechsel eliminieren &#xA; &#xA;delete t1 &#xA;from bisintdata.dbo.xinpsollmiete t1 &#xA;where lid in ( &#xA;select lid from (  &#xA;select *, zr=row_number() over(partition by  xSollstellungsart, xFlaeche, xBeginn, xEnd order by xsmWert desc) &#xA; from BISINTDATA.dbo.xinpSollmiete  &#xA; ) q where zr&gt;1 ) &#xA;go&#xA;-----------------------------------------------------------------------------&#xA;-- künftige 0-sollstellungen bei Vertrag mit Option raus&#xA;-----------------------------------------------------------------------------&#xA;&#xA;with KondEnde as ( &#xA;select xvertrag, xflaeche, xbeginn, xSollstellungsart from ( &#xA;select xvertrag, xflaeche, xende,xSollstellungsart+'_CN' as xSollstellungsart, zr=ROW_NUMBER() over (partition by  xvertrag, xflaeche, xSollstellungsart order by xende desc)  &#xA;, xbeginn = xEnde+1 &#xA;from cit_SollmieteVertrag &#xA;where xende &gt; getdate() &#xA;and xende &lt;&gt; '99991231' &#xA;and CONDVALIDTOX = '99991231' &#xA;and VALUEVALIDTOX = xEnde &#xA;and xvtende is null &#xA;and CONDVALUEX &lt;&gt; 0 &#xA;) q where zr=1 &#xA;) &#xA;delete inp &#xA;from bisintdata.dbo.xinpSollmiete inp &#xA;join Kondende ke on inp.xFlaeche = ke.xFlaeche and inp.xSollstellungsart = ke.xSollstellungsart and inp.xBeginn = ke.xbeginn &#xA;join (select distinct xvertrag, xFlaeche  from bisintdata.dbo.xinpKuendigungsoptionen union select xVertrag, xFlaeche from bisintdata.dbo.xinpVertragsIndex )  opt on ke.xFlaeche = opt.xFlaeche and ke.xVertrag = opt.xvertrag &#xA;where inp.xsmWert = 0 &#xA;&#xA;-----------------------------------------------------------------------------&#xA;-- künftige 0-sollstellungen bei zu Vertragsende / 01.01.2100 entfernen&#xA;-----------------------------------------------------------------------------&#xA;&#xA;delete s&#xA;--select xvtLeerstehend, v.xVertrag, xvtbeginn, v.xvtEnde, v.xvtVertrEndeFestlaufzeit, f.xflAbgangsdatum , s. * &#xA;from bisintdata.dbo.xinpSollmiete s&#xA;inner join bisintdata.dbo.xinpVertrag v on s.xFlaeche = v.xvtFlaeche and s.xBeginn &gt;= v.xvtBeginn&#xA;inner join bisintdata.dbo.xinpFlaeche f on f.xFlaeche = s.xFlaeche&#xA;where 1 = 1 &#xA;and xvtEnde is null&#xA;and (xBeginn = '2100-01-01 00:00:00.000' OR xBeginn = dateadd(d,1,xvtVertrEndeFestlaufzeit))&#xA;and xsmWert = 0 &#xA;and (f.xflAbgangsdatum is null or xBeginn &lt; f.xflAbgangsdatum)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
