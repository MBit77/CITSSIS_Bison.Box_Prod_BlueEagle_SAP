          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"update uFile
set xFondsNr	= map.Fonds_tgt
,	xGesellNr	= map.Gesell_tgt
,	xObjNr		= map.Objekt_tgt
,	xggNr		= map.Gebaeude_tgt
,	xPrefix		= map.Prefix
from " +@[User::cfgIMPDB]+ ".dbo.SAP_VIBDBE uFile
inner join
(	
	select
	  Datenlieferant= xHubID 
	, Prefix		= dbo.BISGET_Keyval_Alpha(xPrefix) 
	, Objekt_tgt	= xobjnr
	, Objekt_src	= xUnitAssetCode
	, Fonds_tgt		= xfondsnr
	, Gesell_src	= xUnitCompanyCode
	, Gesell_tgt	= xgesellschaftcode
	, Gebaeude_tgt	= xggnr
	from dbo.xIntfConfiguration 
	where xImportTenant = 1 
	and ldelete = 0
	and dbo.BISGET_Keyval_Alpha(xTypeOfDelivery) = '" +@[User::cfgTypeOfDelivery]+ "'
	and nullif(xUnitAssetCode, '') is not null
	and nullif(xobjnr, '') is not null
) map on rtrim(uFile.SWENR) = rtrim(map.Objekt_src) and rtrim(uFile.BUKRS) = rtrim(map.Gesell_src) and uFile.xHubID = map.Datenlieferant and uFile.xHubID = map.Datenlieferant
