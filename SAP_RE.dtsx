              <DTS:VariableValue
                DTS:DataType="8">select distinct 
  dtedit				= getdate()
, luserkz				= 'INTERFACE'
, ldelete				= '0'
/* RS - 20230426: wenn mehrere Verträge zu einem Hauptvertrag, dann erzeuge eine laufende Nummer */
--, xVertrag				= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR as varchar(50))
, xVertrag				= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) +'_'+ vtg.RECNNR + case when assAnz.lid is null or NbrVt = '' then '' else '_' + right('00'+ass.NbrVt,2) end as varchar(100))
, xvtFlaeche			= cast(obj.xobjnr +'_'+ case when me.SGENR is null then 'G' else '' end + right(isnull(me.SGENR, me.SGRNR), 3) +'_'+ right(me.SMENR,5) as varchar(50))
, xvtObjekt				= cast(obj.xobjnr as varchar(50))
, xvtAdressnummer		= cast(bp.PARTNER as varchar(50))
, xvtSollstellungstag	= '1'
, xvtLeerstehend		= case when isnull(ISVACANT,'') = 'X' then '1' else '0' end
--, xvtBeginn				= vtg.RECNBEGX
, xvtBeginn				= case when ass.VALIDFROMX &gt; vtg.RECNBEGX then ass.VALIDFROMX else vtg.RECNBEGX  end
, xvtEnde				= case  
								when ass.VALIDTOX &lt; isnull(vtg.RECNENDABSX,'99991231') and ass.VALIDTOX  &lt;&gt; '99991231' then nullif(ass.VALIDTOX,'99991231')   
								WHEN lfz.PESTATE in('X0', 'W0')  then vtg.RECNENDABSX 
                                when kuendobj.NTPER is not null and kuendobj.NTPER between isnull(ass.VALIDFROMX,'17530101') and ass.VALIDTOX  then kuendobj.NTPER 
                                when kuend.NTPER is not null then kuend.NTPER 
								else NULL
                          end

, xvtEnde_intf				= '1'
, xvtVertrEndeFestlaufzeit		= CASE
										/*Verträge mit Status = unbefristet &gt;&gt;&gt; NULL*/
										
										/*Verträge mit Status = Vor Vertragsbeginn &gt;&gt;&gt; nächstes Vertragsende Stammdaten*/
										WHEN lfz.PESTATE = 'A0' then vtg.RECNEND1STX
										/*Verträge mit Status = beendet ODER  gekündigt ODER befristet ohne Verlängerung &gt;&gt;&gt; nächstes Vertragsende NEU*/
										WHEN lfz.PESTATE in('X0', 'W0', 'C0') then vtg.RECNENDABSX 
										/**Verträge mit Status = unbefristete Verlängerung ODER unbefristet &gt;&gt;&gt; nächstes Kündigungsdatum Mieter*/
										WHEN lfz.PESTATE in('RU', 'B0') then lfz.NTALLOWEDPAR
										/*other*/
										ELSE lfz.RNENDNEW
									END
							
/*OLD 
, xvtVertrEndeFestlaufzeit		=	CASE
										/*unbefristet &gt;&gt;&gt; NULL*/
										WHEN lfz.PESTATE = 'B0' then NULL 
										/*Erstes Ende &lt; Beginn &gt;&gt;&gt; NULL*/
										WHEN vtg.RECNEND1STX &lt; case when ass.VALIDFROMX &gt; vtg.RECNBEGX then ass.VALIDFROMX else vtg.RECNBEGX  end then NULL
										ELSE vtg.RECNEND1STX 
									END
*/
, xvtVertrEndeFestlaufzeit_intf	= '1'
, xvertrBranche					= obj.xPrefix +'_'+ vtg.Industry
, xvtBemerkung					= cast(rtrim(vtg.RECNTXT) + case when fKndRgl.rglTxt is not null then  '   &lt;Individuelle Kündigungsregel vorhanden!&gt;   ' + fKndRgl.rglTxt end as varchar(1024))
								+ CHAR(13) + 'Vertragstatus: ' + rtrim(XPESTATE)
, xvertrErstesVertrEnde			= vtg.RECNEND1STX
, xvertrErstesVertrEnde_intf	= '1'
, xvertrstammvertrag			= obj.xobjnr + ' '+ vtg.RECNNR
, xvertrStammvertragBeginn		= vtg.RECNBEGX
, xvertrStammvertragende		= CASE when Kuend.INTRENO is not null and Kuend.NTPER is not null  then convert(datetime,Kuend.NTPER) end  

, xvertrNaechstesVertragsende            
				= CASE  -- MO gekündigt
					WHEN ass.VALIDTOX is Not Null and ass.VALIDTOX &lt;&gt; '99991231' and ass.VALIDTOX &lt; vtg.recnendabsX then ass.VALIDTOX
					WHEN vtg.RECNENDABSX is not null and vtg.recnendabsx &lt;= getdate()  then vtg.RECNENDABSX 
					WHEN lfz.PESTATE in('X0', 'W0')  then vtg.RECNENDABSX 
					-- unbefristet
					WHEN lfz.PESTATE = 'B0' then case when lfz.NTALLOWEDPAR is not null and lfz.NTALLOWEDPAR &lt; isnull(lfz.NTALLOWEDOWN,'99991231') then lfz.NTALLOWEDPAR else lfz.NTALLOWEDOWN end
					-- befristet ohne Verlängerung
					WHEN lfz.PESTATE = 'C0' then 
						case	when lfz.NTALLOWEDPAR is not null and  lfz.NTALLOWEDPAR &lt; isnull(lfz.RNEND1ST, '99991231') then lfz.NTALLOWEDPAR 
								else  lfz.RNEND1ST
						end  
								WHEN lfz.PESTATE in('F0', 'O1', 'O2', 'R', 'RR', 'RU') then 
									case 
										when lfz.RNENDNEW is not null and lfz.RNENDNEW &lt;= isnull(lfz.NTALLOWEDPAR,'99991231') and  lfz.RNENDNEW &lt;= isnull(lfz.NTALLOWEDOWN,'99991231')  
											then lfz.RNENDNEW 
										when lfz.NTALLOWEDPAR is not null and  lfz.NTALLOWEDPAR &lt;= isnull(lfz.RNENDNEW,'99991231') and  lfz.NTALLOWEDPAR &lt;= isnull(lfz.NTALLOWEDOWN,'99991231') 
											then  lfz.NTALLOWEDPAR 
										when lfz.NTALLOWEDOWN is not null and  lfz.NTALLOWEDOWN &lt;= isnull(lfz.RNENDNEW,'99991231') and  lfz.NTALLOWEDOWN &lt;= isnull(lfz.NTALLOWEDPAR,'99991231')  
											then lfz.NTALLOWEDOWN 
									end                                   
				  END
, xvertrNaechstesVertragsende_intf	= '1'
--, xvertrKuendZum				= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), vtg.RECNNOTPER), NULL)
, xvertrKuendZum				= BISDSYSDB.dbo.BIS_Convert_DATE(convert(varchar(500), kuend.NTPER), NULL)
, xvertrKuendZum_intf			= '1'
, xvtKzMWST						= case when BISDSYSDB.dbo.BIS_Convert_DEC(tax.OPTRATE) = 0.00 or tax.OPTRATE is null then 0 else 1 end
, xvtKzMWST_intf				= '1'
, xvertrEingangKuendigung		= kuendobj.NTRECEIPTX
, xvertrEingangKuendigung_intf	= '1'
, xvtMibeso						= cast(	case kndRgl.NTTYPE 
											when 1 then fKuenRgl.XNTPROC 
											when 2 then obj.xPrefix +'_'+ '0000' 
										end as varchar(50)) -- 0000=individuelle Fristregelung
, xvtMibeso_intf				= '1'
, xvertrSondKuendRecht			= case when lfz.NTALLOWEDPAR is not null and lfz.NTALLOWEDPAR &gt; GETDATE() then  nullif(lfz.NTALLOWEDPAR,'99991231') end
, xvertrSondKuendRecht_intf		= '1'
, xvertrExtID					= vtg.INTRENO
, xvertrExtID_intf				= '1'
--, lfz.*
 -- select *
 FROM
(
    SELECT *              
		, RECNBEGX    = bisdsysdb.dbo.BIS_Convert_DATE(nullif(RECNBEG,''), null)
        , RECNENDABSX = bisdsysdb.dbo.BIS_Convert_DATE(nullif(recnendabs,''), null)
        , RECNEND1STX = bisdsysdb.dbo.BIS_Convert_DATE(nullif(RECNEND1ST,''), null)
        , RECNDATX    = case when isdate(replace(RECNDAT, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(RECNDAT,''), null) end
        , RECN2DATX   = case when isdate(replace(RECN2DAT, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(RECN2DAT,''), null) end
    FROM dbo.SAP_VICNCN with(nolock)
)  vtg
JOIN
(
    SELECT xHubID, MANDT, OBJNRSRC, OBJNRTRG, ASSIGNONLYINFO
		, VALIDFROMX = case when isdate(replace(VALIDFROM, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(VALIDFROM,''), null) end
		, VALIDTOX = case when isdate(replace(VALIDTO, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(VALIDTO,''), null) end
        , NbrVt = isnull(nullif(convert(varchar(10),ROW_NUMBER() over(partition by OBJNRSRC, OBJNRTRG order by isnull(validTo,'99991231'))),'1'),'')   
    FROM dbo.SAP_VIBDOBJASS
	WHERE OBJASSTYPE = '10'
) ass ON vtg.xHubid = ass.xHubid and vtg.OBJNR = ass.OBJNRSRC
LEFT JOIN
(
SELECT lid = 1, OBJNRSRC, OBJNRTRG, xhubid, MANDT, Anzahl = count(OBJASSTYPE)
FROM dbo.SAP_VIBDOBJASS WHERE OBJASSTYPE = '10'
GROUP BY OBJNRSRC, OBJNRTRG, xhubid, MANDT
) assAnz on assAnz.OBJNRSRC = ass.OBJNRSRC and assAnz.OBJNRTRG = ass.OBJNRTRG and assAnz.xHubID = ass.xHubID and ass.MANDT = assAnz.MANDT and assAnz.Anzahl &gt; 1
 join           dbo.SAP_vibdro me on vtg.xHubid = me.xHubid and ass.OBJNRTRG = me.OBJNR
 join		    dbo.SAP_VIBDBE obj on me.xHubid = obj.xHubid and me.BUKRS = obj.BUKRS and me.SWENR = obj.SWENR
join	(	select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) 
			from dbo.SAP_vibpobjrel 
			-- externe role ist zu P03 (Vertragspartner gemappt)
			join temp_SAP_mapping_BP map on xPrefix +'_'+ ROLE = map.mapFrom
			where mapTo = 'P03'
		) bp on obj.xHubid = bp.xHubid and vtg.Intreno = bp.INTRENO and bp.R = 1
-- join (select *, R=ROW_NUMBER() over (partition by mandt, intreno, role order by validfrom desc) from dbo.SAP_vibpobjrel where ROLE = ?) bp on vtg.xHubid = bp.xHubid and vtg.Intreno = bp.INTRENO and bp.R = 1
 left join      dbo.SAP_vibdroocc oc on vtg.xHubid = oc.xHubid and  me.INTRENO = oc.INTRENO and vtg.BUKRS = oc.BUKRS and vtg.RECNNR = oc.OCCRECNNR
LEFT JOIN 
(
    select vi.INTRENO, OBJNR = ob.OBJNR , vi.NTREASON, vi.NTPARTY, NTACTIVATEDON
	, NTRECEIPTX = case when isdate(replace(NTRECEIPT, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(NTRECEIPT,''), null) end
	, NTPER = case when isdate(replace(NTPER, '-', '')) = 1 then bisdsysdb.dbo.BIS_Convert_DATE(nullif(NTPER,''), null) end
    from dbo.SAP_VICNNT vi  
    join dbo.SAP_VICNNTOBJREL ob on vi.INTRENO = ob.INTRENO and vi.NTNO = ob.NTNO  
    where vi.NTCATE = 'O' and NTACTIVE = 'X'
) kuendobj on vtg.INTRENO = kuendobj.INTRENO and me.OBJNR = kuendobj.OBJNR 
 left join		dbo.SAP_VICNNT kuend on vtg.xHubid = kuend.xHubid and kuend.INTRENO = vtg.INTRENO and NTACTIVE = 'X' and nullif(NTREVERSEDON, '00000000') is null and NTCATE = 'C' -- Contract
 -- Umsatzsteuer
 left join
( select *, zr=ROW_NUMBER() over(partition by xHubid, MANDT, OBJNR order by BISDSYSDB.dbo.BIS_Convert_DATE(isnull(VALIDFROM, '01.01.2019') , NULL)desc )  
	-- select * 
	from	dbo.SAP_VIITOPTRATE 
	where VALIDTO  &lt;&gt; '99991231' 
) tax  on vtg.xHubid = tax.xHubid and vtg.MANDT = tax.MANDT and vtg.OBJNR = tax.OBJNR and tax.zr = 1 
 -- Verlängerungen
 left join
 (
         select Verl.xHubId,  verl.INTRENO, verl.RNENDNEW
         from SAP_VICNRN Verl
         inner join (select distinct INTRENO, RNNO = MAX(RNNO) OVER (PARTITION BY INTRENO) from SAP_vicnrn where RNACTIVE = 'X' and nullif(RNACTIVATEDON,'') is not null) VerlMax
                 on Verl.INTRENO = VerlMax.INTRENO and Verl.RNNO = VerlMax.RNNO
         where Verl.RNACTIVE = 'X' and nullif(Verl.RNACTIVATEDON,'') is not null
 ) Verl on Verl.INTRENO = vtg.INTRENO
-- Kündigungsregeln
left join SAP_VITMNT kndRgl on kndRgl.xHubID = vtg.xHubID and kndRgl.INTRENO = vtg.INTRENO and kndRgl.NTTYPE in (1,2) and kndRgl.TERMTYPE = '1020'
-- feste Künd. Regel
left join SAP_TIVTMNTPROCT fKuenRgl on fKuenRgl.SPRAS = 'D' and fKuenRgl.NTPROC = kndRgl.NTPROC
-- individuelle Künd. Regel
left join 
(
	select distinct xHubID, INTRENO
		, STUFF((	select  ',' + XNTRULE 
					from [dbo].[SAP_VITMNTPROCREL] b 
					where a.intreno = b.intreno 
					and b.TERMTYPE = '1020'
					for xml path ('')), 1, 1, ''
				) as rglTxt 
	from [dbo].[SAP_VITMNTPROCREL] a
	where TERMTYPE = '1020' 
) fKndRgl on fKndRgl.xHubID = vtg.xHubID and fKndRgl.INTRENO = vtg.INTRENO
-- In SAP ermittelte Vertragsenden
left join   
(
	select distinct xHubID, INTRENO
		, RNEND1ST		= bisdsysdb.dbo.BIS_Convert_DATE(nullif(RNEND1ST,''), null)
		, RNENDNEW		= bisdsysdb.dbo.BIS_Convert_DATE(nullif(RNENDNEW, ''), null)
		, RNENDMAX		= bisdsysdb.dbo.BIS_Convert_DATE(nullif(RNENDMAX,'') , null)
		, NTALLOWEDPAR	= bisdsysdb.dbo.BIS_Convert_DATE(nullif(NTALLOWEDPAR,''), null)
		, NTALLOWEDOWN	= bisdsysdb.dbo.BIS_Convert_DATE(nullif(NTALLOWEDOWN,''), null)
		--, NTPERINACTIVE = convert(datetime,nullif(NTPERINACTIVE,'1900-01-01 00:00:00.000'))             
		, case PESTATE
			when 'Vor Vertragsbeginn' then 'A0'
			when 'Unbefristet' then 'B0'
			when 'Befristet (ohne Verlängerung)' then 'C0'
			when 'In Grundlaufzeit' then 'F0'
			when 'In 1. Option' then 'O1'
			when 'In 2. Option' then 'O2'
			when 'In 3. Option' then 'O3'
			when 'In ein- oder mehrmaliger Verlängerung' then 'R'
			when 'In wiederkehrender Verlängerung' then  'RR'
			when 'In unbeschränkter Verlängerung' then  'RU'
			when 'Gekündigt' then 'W0'
			when 'Beendet' then 'X0'
			end as PESTATE
			, PESTATE as XPESTATE
	-- select *
	from dbo.SAP_XKUE
) lfz on vtg.INTRENO = lfz.INTRENO and vtg.xHubID = lfz.xHubID
where obj.xobjnr is not null 
and isnull(ass.ASSIGNONLYINFO, '') != 'X'</DTS:VariableValue>
